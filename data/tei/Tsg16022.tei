<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="--++---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Deh, poi-ché+ sde+-gni me, com’e+gli_è va+go =A+ -->
					<seg type="syl" met="-">Deh, </seg>
					<seg type="syl" met="-">poi</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-"></seg>
					<seg type="syl" met="-">gni </seg>
					<seg type="syl" met="-">me, </seg>
					<seg type="syl" met="+">com’e</seg>
					<seg type="syl" met="-">gli<c met="_"> </c>è </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">go</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Mi-rar+ tu_al-men+ po-tes+si_il pro+prio vol+to: =B -->
					<seg type="syl" met="-">Mi</seg>
					<seg type="syl" met="+">rar </seg>
					<seg type="syl" met="-">tu<c met="_"> </c>al</seg>
					<seg type="syl" met="+">men </seg>
					<seg type="syl" met="-">po</seg>
					<seg type="syl" met="+">tes</seg>
					<seg type="syl" met="-">si<c met="_"> </c>il </seg>
					<seg type="syl" met="+">pro</seg>
					<seg type="syl" met="-">prio </seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">to:</seg>
				</l>
				<l rhyme="a" met="-----+---+-" n="endecasillabo">
					<!-- INPUT: Ché_’l guar-do tuo, ch’al-tro+ve non è pa+go, =A -->
					<seg type="syl" met="-">Ché<c met="_"> </c>’l </seg>
					<seg type="syl" met="-">guar</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">tuo, </seg>
					<seg type="syl" met="-">ch’al</seg>
					<seg type="syl" met="+">tro</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">è </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">go,</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Gio-i-reb+be fe-li+ce_in se ri-vol+to. =B -->
					<seg type="syl" met="-">Gio</seg>
					<seg type="syl" met="-">i</seg>
					<seg type="syl" met="+">reb</seg>
					<seg type="syl" met="-">be </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>in </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="a" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Non può spec+chio ri-trar+ sì dol+ce_i-ma+go: =A -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">può </seg>
					<seg type="syl" met="+">spec</seg>
					<seg type="syl" met="-">chio </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">trar </seg>
					<seg type="syl" met="-">sì </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>i</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">go:</seg>
				</l>
				<l rhyme="b" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Né_in pic+ciol ve+tro_è_un pa-ra-di+so_ac-col+to. =B -->
					<seg type="syl" met="-">Né<c met="_"> </c>in </seg>
					<seg type="syl" met="+">pic</seg>
					<seg type="syl" met="-">ciol </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">tro<c met="_"> </c>è<c met="_"> </c>un </seg>
					<seg type="syl" met="-">pa</seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">so<c met="_"> </c>ac</seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="c" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Spec+chio t’è de+gno_il Cie+lo,_e nel+le stel+le =C -->
					<seg type="syl" met="+">Spec</seg>
					<seg type="syl" met="-">chio </seg>
					<seg type="syl" met="-">t’è </seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>il </seg>
					<seg type="syl" met="+">Cie</seg>
					<seg type="syl" met="-">lo,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">nel</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">stel</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Puoi ri-guar-dar+ le tue sem-bian+ze bel+le. =C -->
					<seg type="syl" met="-">Puoi </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">guar</seg>
					<seg type="syl" met="+">dar </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">tue </seg>
					<seg type="syl" met="-">sem</seg>
					<seg type="syl" met="+">bian</seg>
					<seg type="syl" met="-">ze </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">le.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
