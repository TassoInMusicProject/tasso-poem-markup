<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Pas+sa pur que+sto pet+to,_e fe+ri scem+pi =A+ -->
					<seg type="syl" met="+">Pas</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">scem</seg>
					<seg type="syl" met="-">pi</seg>
				</l>
				<l rhyme="b" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: Col fer+ro tuo cru-del+ fà del mio co+re. =B -->
					<seg type="syl" met="-">Col </seg>
					<seg type="syl" met="+">fer</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">del </seg>
					<seg type="syl" met="-">fà </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ma for+se,_u-sa+ta_a’ fat+ti_a-tro+ci_ed em+pi, =A+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se,<c met="_"> </c>u</seg>
					<seg type="syl" met="+">sa</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>a’ </seg>
					<seg type="syl" met="+">fat</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>a</seg>
					<seg type="syl" met="+">tro</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">em</seg>
					<seg type="syl" met="-">pi,</seg>
				</l>
				<l rhyme="b" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: Sti+mi pie-tà+ dar mor+te_al mio do-lo+re. =B -->
					<seg type="syl" met="+">Sti</seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">tà </seg>
					<seg type="syl" met="-">dar </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te<c met="_"> </c>al </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Dun+que_i’ vi-vrò+ tra me-mo-ran+di_e-sem+pi =A+ -->
					<seg type="syl" met="+">Dun</seg>
					<seg type="syl" met="-">que<c met="_"> </c>i’ </seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">vrò </seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="-">me</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ran</seg>
					<seg type="syl" met="-">di<c met="_"> </c>e</seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pi</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Mi+se-ro mo+stro d’in-fe-li+ce_a-mo+re: =B -->
					<seg type="syl" met="+">Mi</seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="-">d’in</seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re:</seg>
				</l>
				<l rhyme="c" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Mi+se-ro mo+stro,_a cui sol pe+na_è de+gna =C -->
					<seg type="syl" met="+">Mi</seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">stro,<c met="_"> </c>a </seg>
					<seg type="syl" met="-">cui </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">na<c met="_"> </c>è </seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gna</seg>
				</l>
				<l rhyme="c" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Del-l’im-men+sa_em-pie-tà+ la vi+ta_in-de+gna. =C -->
					<seg type="syl" met="-">Del</seg>
					<seg type="syl" met="-">l’im</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>em</seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">tà </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>in</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gna.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
