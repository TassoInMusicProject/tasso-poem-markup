<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-----+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Fe-li+ce me, se nel mo-rir+ non re+co =A+ -->
					<seg type="syl" met="-">Fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">me, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">co</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Que+sta mia pe+ste_ad in-fet-tar+ l’In-fer+no. =B -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">ste<c met="_"> </c>ad </seg>
					<seg type="syl" met="-">in</seg>
					<seg type="syl" met="-">fet</seg>
					<seg type="syl" met="+">tar </seg>
					<seg type="syl" met="-">l’In</seg>
					<seg type="syl" met="+">fer</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
				<l rhyme="a" met="+--++--+-+-" n="endecasillabo">
					<!-- INPUT: Re+sti-ne_A-mor;+ ven+ga sol sde+gno_or me+co, =A -->
					<seg type="syl" met="+">Re</seg>
					<seg type="syl" met="-">sti</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor; </seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">ga </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>or </seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">co,</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: E sia del-l’om+bra mia com-pa+gno_e-ter+no: =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">sia </seg>
					<seg type="syl" met="-">del</seg>
					<seg type="syl" met="+">l’om</seg>
					<seg type="syl" met="-">bra </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">com</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>e</seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">no:</seg>
				</l>
				<l rhyme="a" met="--+----+---" n="endecasillabo">
					<!-- INPUT: O ri-tor+ni con lui dal re+gno cie-co =A -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">tor</seg>
					<seg type="syl" met="-">ni </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">lui </seg>
					<seg type="syl" met="-">dal </seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="-">cie</seg>
					<seg type="syl" met="-">co</seg>
				</l>
				<l rhyme="b" met="--+----+-+-" n="endecasillabo">
					<!-- INPUT: A co-lui+ che di me fé l’em+pio scher+no: =B -->
					<seg type="syl" met="-">A </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">lui </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">fé </seg>
					<seg type="syl" met="+">l’em</seg>
					<seg type="syl" met="-">pio </seg>
					<seg type="syl" met="+">scher</seg>
					<seg type="syl" met="-">no:</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E se gli mo+stri tal, che,_in fe+re not+ti, =C+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">gli </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">stri </seg>
					<seg type="syl" met="-">tal, </seg>
					<seg type="syl" met="-">che,<c met="_"> </c>in </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="+">not</seg>
					<seg type="syl" met="-">ti,</seg>
				</l>
				<l rhyme="c" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: Ab+bia ri-po+si_or-ri+bi-li_e_in-ter-rot+ti. =C -->
					<seg type="syl" met="+">Ab</seg>
					<seg type="syl" met="-">bia </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">po</seg>
					<seg type="syl" met="-">si<c met="_"> </c>or</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">bi</seg>
					<seg type="syl" met="-">li<c met="_"> </c>e<c met="_"> </c>in</seg>
					<seg type="syl" met="-">ter</seg>
					<seg type="syl" met="+">rot</seg>
					<seg type="syl" met="-">ti.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
