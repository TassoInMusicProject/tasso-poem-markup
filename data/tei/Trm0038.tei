<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--++--+-+-" n="endecasillabo">
					<!-- INPUT: Sta+va-si_A-mor+ qua+si_in suo re+gno_as-si+so =A -->
					<seg type="syl" met="+">Sta</seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="-">si<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="+">qua</seg>
					<seg type="syl" met="-">si<c met="_"> </c>in </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>as</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">so</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Nel se-ren+ di due lu+ci_ar-den+ti_ed al+me, =B -->
					<seg type="syl" met="-">Nel </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">ren </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">due </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">me,</seg>
				</l>
				<l rhyme="b" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Mil+le fa-mo+se_in-se+gne_e mil+le pal+me =B -->
					<seg type="syl" met="+">Mil</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">se<c met="_"> </c>in</seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">gne<c met="_"> </c>e </seg>
					<seg type="syl" met="+">mil</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">pal</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="a" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Spie-gan+do_in un se-re+no_e chia+ro vi+so; =A -->
					<seg type="syl" met="-">Spie</seg>
					<seg type="syl" met="+">gan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>in </seg>
					<seg type="syl" met="-">un </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">no<c met="_"> </c>e </seg>
					<seg type="syl" met="+">chia</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">so;</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Quan+do ri-vol+to_à me, ch'in-ten+to_e fi+so =A -->
					<seg type="syl" met="+">Quan</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">to<c met="_"> </c>à </seg>
					<seg type="syl" met="-">me, </seg>
					<seg type="syl" met="-">ch'in</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">to<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">so</seg>
				</l>
				<l rhyme="b" met="-+---+-+-+--" n="12">
					<!-- INPUT: Mi-ra+va le sue ric+che_e ca+re sal+me: &mdash; =B -->
					<seg type="syl" met="-">Mi</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">va </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">sue </seg>
					<seg type="syl" met="+">ric</seg>
					<seg type="syl" met="-">che<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="+">sal</seg>
					<seg type="syl" met="-">me: </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="b" met="-+--+--+-+-+-" n="13">
					<!-- INPUT: Or can+ta &mdash; dis+se &mdash; co+me_i co+ri_e l'al+me =B -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="+">can</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="+">dis</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">me<c met="_"> </c>i </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>e </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="a" met="---+-++--+-" n="endecasillabo">
					<!-- INPUT: E_'l tuo me-des+mo_an-co+ra_ab+bia con-qui+so; =A -->
					<seg type="syl" met="-">E<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="-">me</seg>
					<seg type="syl" met="+">des</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>an</seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="+">ra<c met="_"> </c>ab</seg>
					<seg type="syl" met="-">bia </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">qui</seg>
					<seg type="syl" met="-">so;</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="-+---++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Né s'o+da ri-so-nar+ l'ar+me di Mar+te =C+ -->
					<seg type="syl" met="-">Né </seg>
					<seg type="syl" met="+">s'o</seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">nar </seg>
					<seg type="syl" met="+">l'ar</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">Mar</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="d" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: La vo+ce tua, ma l'al+ta_e chia+ra glo+ria, =D -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">tua, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>e </seg>
					<seg type="syl" met="+">chia</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="+">glo</seg>
					<seg type="syl" met="-">ria,</seg>
				</l>
				<l rhyme="e" met="--++-+---+--" n="12">
					<!-- INPUT: E_i di-vin+ pre+gi no+stri_e di co-ste+i. &mdash; =E -->
					<seg type="syl" met="-">E<c met="_"> </c>i </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">vin </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stri<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">ste</seg>
					<seg type="syl" met="-">i. </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="d" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Co-sì_ad+di-vien+ che ne l'al-trui+ vit-to+ria =D -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">sì<c met="_"> </c>ad</seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">vien </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">l'al</seg>
					<seg type="syl" met="+">trui </seg>
					<seg type="syl" met="-">vit</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">ria</seg>
				</l>
				<l rhyme="e" met="+----+-+-+-" n="endecasillabo">
					<!-- INPUT: Can+ti mia ser-vi-tu+te_e_i lac+ci mi+ei =E -->
					<seg type="syl" met="+">Can</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">ser</seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">lac</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">ei</seg>
				</l>
				<l rhyme="c" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: E tes+sa de gli_af-fan+ni_i-sto+rie_in car+te. =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">tes</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">gli<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fan</seg>
					<seg type="syl" met="-">ni<c met="_"> </c>i</seg>
					<seg type="syl" met="+">sto</seg>
					<seg type="syl" met="-">rie<c met="_"> </c>in </seg>
					<seg type="syl" met="+">car</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
