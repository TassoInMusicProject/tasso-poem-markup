<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="---+-+-+---" n="endecasillabo" enjamb="yes">
					<!-- INPUT: O pur ver-rò+ là do+ve se+te, e voi =A+ -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="-">là </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">te, </seg>
					<seg type="syl" met="-">e </seg>
					<seg type="syl" met="-">voi</seg>
				</l>
				<l rhyme="b" met="+-+--++-+-+-" n="12">
					<!-- INPUT: Me+co_a-vrò+, s’an-co+ se+te,_a-ma+te spo+glie. =B -->
					<seg type="syl" met="+">Me</seg>
					<seg type="syl" met="-">co<c met="_"> </c>a</seg>
					<seg type="syl" met="+">vrò</seg>
					<seg type="syl" met="-">, </seg>
					<seg type="syl" met="-">s’an</seg>
					<seg type="syl" met="+">co </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">spo</seg>
					<seg type="syl" met="-">glie.</seg>
				</l>
				<l rhyme="a" met="-++---+-+--" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ma s’e+gli_av+vien che i va+ghi mem+bri suoi =A+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">s’e</seg>
					<seg type="syl" met="+">gli<c met="_"> </c>av</seg>
					<seg type="syl" met="-">vien </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">i </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ghi </seg>
					<seg type="syl" met="+">mem</seg>
					<seg type="syl" met="-">bri </seg>
					<seg type="syl" met="-">suoi</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Sta+ti sian ci+bo di fe-ri+ne vo+glie; =B -->
					<seg type="syl" met="+">Sta</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">sian </seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">bo </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">glie;</seg>
				</l>
				<l rhyme="a" met="---+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Vuò che la boc+ca stes+sa_an-co+ me in+goi, =A+ -->
					<seg type="syl" met="-">Vuò </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">boc</seg>
					<seg type="syl" met="-">ca </seg>
					<seg type="syl" met="+">stes</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>an</seg>
					<seg type="syl" met="+">co </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">in</seg>
					<seg type="syl" met="-">goi,</seg>
				</l>
				<l rhyme="b" met="-+-+-----+-" n="endecasillabo">
					<!-- INPUT: E_’l ven+tre chiu+da me che lor rac-co+glie. =B -->
					<seg type="syl" met="-">E<c met="_"> </c>’l </seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="+">chiu</seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">lor </seg>
					<seg type="syl" met="-">rac</seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">glie.</seg>
				</l>
				<l rhyme="c" met="--+---+--+-" n="endecasillabo">
					<!-- INPUT: O-no-ra+ta per me tom+ba_e fe-li+ce, =C -->
					<seg type="syl" met="-">O</seg>
					<seg type="syl" met="-">no</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">tom</seg>
					<seg type="syl" met="-">ba<c met="_"> </c>e </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce,</seg>
				</l>
				<l rhyme="c" met="-+--+----+-" n="endecasillabo">
					<!-- INPUT: O-vun+que sia, s’es+ser con lor mi li+ce. =C -->
					<seg type="syl" met="-">O</seg>
					<seg type="syl" met="+">vun</seg>
					<seg type="syl" met="-">que </seg>
					<seg type="syl" met="-">sia, </seg>
					<seg type="syl" met="+">s’es</seg>
					<seg type="syl" met="-">ser </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">lor </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
