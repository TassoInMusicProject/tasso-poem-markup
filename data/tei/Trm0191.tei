	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Ar+si men+tre m'a-ma+ste, =a -->
					<seg type="syl" met="+">Ar</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">m'a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ste,</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario">
					<!-- INPUT: Ed al cor-te+se_af-fet+to =b -->
					<seg type="syl" met="-">Ed </seg>
					<seg type="syl" met="-">al </seg>
					<seg type="syl" met="-">cor</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">se<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Pa-gò+ tri-bu+to di so-spi+ri_il pet+to: =B -->
					<seg type="syl" met="-">Pa</seg>
					<seg type="syl" met="+">gò </seg>
					<seg type="syl" met="-">tri</seg>
					<seg type="syl" met="+">bu</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>il </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to:</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ma poi che_il vo+stro_a-mo+re =c+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">poi </seg>
					<seg type="syl" met="-">che<c met="_"> </c>il </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stro<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="-+-+-+-" n="settenario">
					<!-- INPUT: Vol-ge+ste_in al+tra par+te, =d -->
					<seg type="syl" met="-">Vol</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">ste<c met="_"> </c>in </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="d" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Don+na, s'e-stin+se_an-co_il+ mio fo+co_in par+te. =D -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na, </seg>
					<seg type="syl" met="-">s'e</seg>
					<seg type="syl" met="+">stin</seg>
					<seg type="syl" met="-">se<c met="_"> </c>an</seg>
					<seg type="syl" met="+">co<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="-">co<c met="_"> </c>in </seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
				<l rhyme="c" met="+--+-+-" n="settenario">
					<!-- INPUT: Sor+se poi no+vo_ar-do+re, =c -->
					<seg type="syl" met="+">Sor</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">poi </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">vo<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="e" met="-+---+-" n="settenario">
					<!-- INPUT: Ma fiam+ma fu di sde+gno =e -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">fiam</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno</seg>
				</l>
				<l rhyme="e" met="-+-----+-+-" n="endecasillabo">
					<!-- INPUT: Per-ché_'l+ mio don di voi sti-ma+ste_in-de+gno: =E -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">don </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">sti</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ste<c met="_"> </c>in</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gno:</seg>
				</l>
				<l rhyme="f" met="+--+-+-" n="settenario">
					<!-- INPUT: Que+sta sec-cò+ le chio+me =f -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="-">sec</seg>
					<seg type="syl" met="+">cò </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">chio</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="f" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Del lau+ro_on-d'o-no-ra+va_il vo+stro no+me. =F -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="+">lau</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>on</seg>
					<seg type="syl" met="-">d'o</seg>
					<seg type="syl" met="-">no</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">va<c met="_"> </c>il </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">me.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
