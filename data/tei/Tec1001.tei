					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="b" met="-+-+-+++-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Qual mu+to pe+sce_o qua+le_è+ roz+zo_ar-men+to =B+ -->
					<seg type="syl" met="-">Qual </seg>
					<seg type="syl" met="+">mu</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">sce<c met="_"> </c>o </seg>
					<seg type="syl" met="+">qua</seg>
					<seg type="syl" met="+">le<c met="_"> </c>è </seg>
					<seg type="syl" met="+">roz</seg>
					<seg type="syl" met="-">zo<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="--+--++--+-" n="endecasillabo">
					<!-- INPUT: Che non fac+cia d'a-mor+ qual+che con-cen+to? =B -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">fac</seg>
					<seg type="syl" met="-">cia </seg>
					<seg type="syl" met="-">d'a</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="+">qual</seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">to?</seg>
