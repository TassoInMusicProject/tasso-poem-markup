	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Già+ fu mia dol+ce spe+me =a+ -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="b" met="-++--+-" n="settenario">
					<!-- INPUT: As-sai+ de+bi-le_e len+ta, =b -->
					<seg type="syl" met="-">As</seg>
					<seg type="syl" met="+">sai </seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">bi</seg>
					<seg type="syl" met="-">le<c met="_"> </c>e </seg>
					<seg type="syl" met="+">len</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="b" met="-+-++--+-+-" n="endecasillabo">
					<!-- INPUT: Or cre+sce sì+ ch'el+la pia-cer+ di-ven+ta. =B -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">sce </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">ch'el</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">pia</seg>
					<seg type="syl" met="+">cer </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">ta.</seg>
				</l>
				<l rhyme="a" met="--++-+-" n="settenario">
					<!-- INPUT: Ma, per-ch'io+ spe+ri_in-sie+me =a -->
					<seg type="syl" met="-">Ma, </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ch'io </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>in</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="c" met="-++--+-" n="settenario">
					<!-- INPUT: E_in-sie+me_ab+bia di-let+to. =c -->
					<seg type="syl" met="-">E<c met="_"> </c>in</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="+">me<c met="_"> </c>ab</seg>
					<seg type="syl" met="-">bia </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="c" met="---+-++--+-" n="endecasillabo">
					<!-- INPUT: Mai non a-dem+pie_A-mor+ o+gni mio_af-fet+to; =C -->
					<seg type="syl" met="-">Mai </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">dem</seg>
					<seg type="syl" met="-">pie<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="+">o</seg>
					<seg type="syl" met="-">gni </seg>
					<seg type="syl" met="-">mio<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fet</seg>
					<seg type="syl" met="-">to;</seg>
				</l>
				<l rhyme="d" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: E sem+pre_il mio pia-ce+re =d+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pre<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">pia</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Tem-pran+do va, per-ch'io+ mag-gior+ lo spe+re. =D -->
					<seg type="syl" met="-">Tem</seg>
					<seg type="syl" met="+">pran</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">va, </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ch'io </seg>
					<seg type="syl" met="-">mag</seg>
					<seg type="syl" met="+">gior </seg>
					<seg type="syl" met="-">lo </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
