					<seg type="syl" met="-">ro</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ce+da,_o lu+ci se-re+ne, =b+ -->
					<seg type="syl" met="+">Ce</seg>
					<seg type="syl" met="-">da,<c met="_"> </c>o </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">ne,</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Qual più+ bel ne+gro_I-ta+lia_in pre+gio tie+ne. =B -->
					<seg type="syl" met="-">Qual </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="+">ne</seg>
