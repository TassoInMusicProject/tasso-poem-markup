					<seg type="syl" met="-">in</seg>
					<seg type="syl" met="+">fer</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
				<l rhyme="d" met="-+-+-+-" n="settenario">
					<!-- INPUT: Az-zur+ro_è_'l cie+lo_e-ter+no =d -->
					<seg type="syl" met="-">Az</seg>
					<seg type="syl" met="+">zur</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>è<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo<c met="_"> </c>e</seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="e" met="--++-+---+-" n="endecasillabo">
					<!-- INPUT: E quel ch'è+ bel+lo_il bel+lo_ha sol da lu+i: =E -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="+">ch'è </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">lo<c met="_"> </c>il </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">lo<c met="_"> </c>ha </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">i:</seg>
				</l>
				<l rhyme="e" met="-++--+-+-+-" n="endecasillabo">
					<!-- INPUT: Ei bel+lo_è+ sol per-ch'as+so-mi+glia_a vu+i. =E -->
					<seg type="syl" met="-">Ei </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="+">lo<c met="_"> </c>è </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ch'as</seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">glia<c met="_"> </c>a </seg>
					<seg type="syl" met="+">vu</seg>
					<seg type="syl" met="-">i.</seg>
