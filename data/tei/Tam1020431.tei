	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: E be-vea+ da' suoi lu+mi =a+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">be</seg>
					<seg type="syl" met="+">vea </seg>
					<seg type="syl" met="-">da' </seg>
					<seg type="syl" met="-">suoi </seg>
