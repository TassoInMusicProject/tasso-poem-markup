					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">va</seg>
				</l>
				<l rhyme="f" met="--+--+-" n="settenario">
					<!-- INPUT: La ca-gion+ de_i so-spi+ri. =f -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">ca</seg>
					<seg type="syl" met="+">gion </seg>
					<seg type="syl" met="-">de<c met="_"> </c>i </seg>
