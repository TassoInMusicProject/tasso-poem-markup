					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ri.</seg>
				</l>
				<l rhyme="g" met="-+-+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Co-sì+ fui pri+ma_a-man+te che_in-ten-des+si =G+ -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">fui </seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">che<c met="_"> </c>in</seg>
					<seg type="syl" met="-">ten</seg>
					<seg type="syl" met="+">des</seg>
					<seg type="syl" met="-">si</seg>
				</l>
				<l rhyme="h" met="-+-+-+-" n="settenario">
					<!-- INPUT: che co+sa fos+se_A-mo+re. =h -->
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="+">fos</seg>
					<seg type="syl" met="-">se<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
				<l rhyme="i" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: Ben me n'ac-cor+si_al-fi+ne:_et in qual mo+do, =I -->
					<seg type="syl" met="-">Ben </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">n'ac</seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">si<c met="_"> </c>al</seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">ne:<c met="_"> </c>et </seg>
					<seg type="syl" met="-">in </seg>
					<seg type="syl" met="-">qual </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">do,</seg>
				</l>
				<l rhyme="j" met="+--+-+-" n="settenario">
					<!-- INPUT: Ho+ra m'a-scol+ta,_e no+ta. =j -->
					<seg type="syl" met="+">Ho</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">m'a</seg>
					<seg type="syl" met="+">scol</seg>
					<seg type="syl" met="-">ta,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ta.</seg>
