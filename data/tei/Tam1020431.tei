					<seg type="syl" met="-">mi</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: U+na stra+nia- dol-cez+za, =b+ -->
					<seg type="syl" met="+">U</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">nia </seg>
					<seg type="syl" met="-">dol</seg>
					<seg type="syl" met="+">cez</seg>
					<seg type="syl" met="-">za,</seg>
				</l>
				<l rhyme="c" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Che la-scia+va nel fi+ne =c+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">la</seg>
					<seg type="syl" met="+">scia</seg>
					<seg type="syl" met="-">va </seg>
