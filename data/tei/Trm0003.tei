<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+-++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Su l'am+pia fron+te_il cre+spo_o+ro lu-cen+te =A+ -->
					<seg type="syl" met="-">Su </seg>
					<seg type="syl" met="+">l'am</seg>
					<seg type="syl" met="-">pia </seg>
					<seg type="syl" met="+">fron</seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="+">spo<c met="_"> </c>o</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="-">lu</seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="+--+--++-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Spar+so_on-deg-gia+va,_e de' be+gli_oc+chi_il rag+gio =B+ -->
					<seg type="syl" met="+">Spar</seg>
					<seg type="syl" met="-">so<c met="_"> </c>on</seg>
					<seg type="syl" met="-">deg</seg>
					<seg type="syl" met="+">gia</seg>
					<seg type="syl" met="-">va,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">de' </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi<c met="_"> </c>il </seg>
					<seg type="syl" met="+">rag</seg>
					<seg type="syl" met="-">gio</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Al ter-re+no_ad-du-cea+ fio-ri+to mag+gio, =B -->
					<seg type="syl" met="-">Al </seg>
					<seg type="syl" met="-">ter</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">no<c met="_"> </c>ad</seg>
					<seg type="syl" met="-">du</seg>
					<seg type="syl" met="+">cea </seg>
					<seg type="syl" met="-">fio</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">mag</seg>
					<seg type="syl" met="-">gio,</seg>
				</l>
				<l rhyme="a" met="-+-++--+-+-" n="endecasillabo">
					<!-- INPUT: E lu+glio_a_i co+ri_ol+tre mi-su+ra_ar-den+te: =A -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">glio<c met="_"> </c>a<c met="_"> </c>i </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="+">ri<c met="_"> </c>ol</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">su</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">te:</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="a" met="-+-+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Nel bian+co se+no_A-mor+ vez-zo-sa-men+te =A+ -->
					<seg type="syl" met="-">Nel </seg>
					<seg type="syl" met="+">bian</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">no<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">vez</seg>
					<seg type="syl" met="-">zo</seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Scher-za+va,_e non o-sò+ di far+gli_ol-trag+gio; =B -->
					<seg type="syl" met="-">Scher</seg>
					<seg type="syl" met="+">za</seg>
					<seg type="syl" met="-">va,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">o</seg>
					<seg type="syl" met="+">sò </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">far</seg>
					<seg type="syl" met="-">gli<c met="_"> </c>ol</seg>
					<seg type="syl" met="+">trag</seg>
					<seg type="syl" met="-">gio;</seg>
				</l>
				<l rhyme="b" met="-+---+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E l'au+ra del par-lar+ cor-te+se_e sag+gio =B+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">l'au</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="+">lar </seg>
					<seg type="syl" met="-">cor</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">se<c met="_"> </c>e </seg>
					<seg type="syl" met="+">sag</seg>
					<seg type="syl" met="-">gio</seg>
				</l>
				<l rhyme="a" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Fra le ro+se spi-rar+ s'u-dia+ so-ven+te. =A -->
					<seg type="syl" met="-">Fra </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">spi</seg>
					<seg type="syl" met="+">rar </seg>
					<seg type="syl" met="-">s'u</seg>
					<seg type="syl" met="+">dia </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="--+--+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Io, che for+ma ce-le+ste_in ter+ra scòr+si, =C+ -->
					<seg type="syl" met="-">Io, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">ce</seg>
					<seg type="syl" met="+">le</seg>
					<seg type="syl" met="-">ste<c met="_"> </c>in </seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="+">scòr</seg>
					<seg type="syl" met="-">si,</seg>
				</l>
				<l rhyme="d" met="-+-+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Rin-chiu+si_i lu+mi,_e dis+si:_Ahi, co-me_è stol+to =D+ -->
					<seg type="syl" met="-">Rin</seg>
					<seg type="syl" met="+">chiu</seg>
					<seg type="syl" met="-">si<c met="_"> </c>i </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">mi,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">dis</seg>
					<seg type="syl" met="-">si:<c met="_"> </c>Ahi, </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="-">me<c met="_"> </c>è </seg>
					<seg type="syl" met="+">stol</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="d" met="+---+--+-+-" n="endecasillabo">
					<!-- INPUT: Sguar+do che_'n lei sia+ d'af-fi-sar+si_ar-di+to! =D -->
					<seg type="syl" met="+">Sguar</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">che<c met="_"> </c>'n </seg>
					<seg type="syl" met="-">lei </seg>
					<seg type="syl" met="+">sia </seg>
					<seg type="syl" met="-">d'af</seg>
					<seg type="syl" met="-">fi</seg>
					<seg type="syl" met="+">sar</seg>
					<seg type="syl" met="-">si<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">to!</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Ma de l'al+tro pe-ri+glio non m'ac-còr+si: =C -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">tro </seg>
					<seg type="syl" met="-">pe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">glio </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">m'ac</seg>
					<seg type="syl" met="+">còr</seg>
					<seg type="syl" met="-">si:</seg>
				</l>
				<l rhyme="d" met="-----+---+-" n="endecasillabo">
					<!-- INPUT: Ché mi fu per l'o-rec+chie_il cor fe-ri+to =D -->
					<seg type="syl" met="-">Ché </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">l'o</seg>
					<seg type="syl" met="+">rec</seg>
					<seg type="syl" met="-">chie<c met="_"> </c>il </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="d" met="-+-++--+-+-" n="endecasillabo">
					<!-- INPUT: E_i det+ti_an-da+ro_o+ve non giun+se_in vol+to. =D -->
					<seg type="syl" met="-">E<c met="_"> </c>i </seg>
					<seg type="syl" met="+">det</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>an</seg>
					<seg type="syl" met="+">da</seg>
					<seg type="syl" met="+">ro<c met="_"> </c>o</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">giun</seg>
					<seg type="syl" met="-">se<c met="_"> </c>in </seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
