<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-----+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: O Mu+sa, tu che di ca-du+chi_al-lo+ri =A+ -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">Mu</seg>
					<seg type="syl" met="-">sa, </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">ca</seg>
					<seg type="syl" met="+">du</seg>
					<seg type="syl" met="-">chi<c met="_"> </c>al</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Non cir-con+di la fron+te_in E-li-co+na, =B -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">cir</seg>
					<seg type="syl" met="+">con</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fron</seg>
					<seg type="syl" met="-">te<c met="_"> </c>in </seg>
					<seg type="syl" met="-">E</seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">na,</seg>
				</l>
				<l rhyme="a" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Ma su nel Cie+lo_in fra_i be-a+ti co+ri =A -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">su </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">Cie</seg>
					<seg type="syl" met="-">lo<c met="_"> </c>in </seg>
					<seg type="syl" met="-">fra<c met="_"> </c>i </seg>
					<seg type="syl" met="-">be</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Hai di stel+le_im-mor-ta+li_au-rea+ co-ro+na, =B -->
					<seg type="syl" met="-">Hai </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">stel</seg>
					<seg type="syl" met="-">le<c met="_"> </c>im</seg>
					<seg type="syl" met="-">mor</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">li<c met="_"> </c>au</seg>
					<seg type="syl" met="+">rea </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">na,</seg>
				</l>
				<l rhyme="a" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Tu spi+ra_al pet+to mio ce-le+sti_ar-do+ri, =A -->
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>al </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">ce</seg>
					<seg type="syl" met="+">le</seg>
					<seg type="syl" met="-">sti<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Tu ri-schiar+a_il mio can+to,_e tu per-do+na =B+ -->
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">schiar</seg>
					<seg type="syl" met="-">a<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">can</seg>
					<seg type="syl" met="-">to,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">na</seg>
				</l>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: S’in-tes+so fre+gi_al ver, s’ad-or+no_in pa+rte =C -->
					<seg type="syl" met="-">S’in</seg>
					<seg type="syl" met="+">tes</seg>
					<seg type="syl" met="-">so </seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">gi<c met="_"> </c>al </seg>
					<seg type="syl" met="-">ver, </seg>
					<seg type="syl" met="-">s’ad</seg>
					<seg type="syl" met="+">or</seg>
					<seg type="syl" met="-">no<c met="_"> </c>in </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">rte</seg>
				</l>
				<l rhyme="c" met="+--+-----+-" n="endecasillabo">
					<!-- INPUT: D’al+tri di-let+ti, che de’ tuoi, le ca+rte. =C -->
					<seg type="syl" met="+">D’al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">ti, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">de’ </seg>
					<seg type="syl" met="-">tuoi, </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">rte.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
