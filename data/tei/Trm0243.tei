	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+-+-" n="settenario">
					<!-- INPUT: Fug-gìa+ di pog+gio_in pog+gio =a -->
					<seg type="syl" met="-">Fug</seg>
					<seg type="syl" met="+">gìa </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">pog</seg>
					<seg type="syl" met="-">gio<c met="_"> </c>in </seg>
					<seg type="syl" met="+">pog</seg>
					<seg type="syl" met="-">gio</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: La mia dol+ce ne-mi+ca, =b -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">ne</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">ca,</seg>
				</l>
				<l rhyme="b" met="-+---++--+-" n="endecasillabo">
					<!-- INPUT: Ed es+sa mi se-guia+ bel+la_e pu-di+ca. =B -->
					<seg type="syl" met="-">Ed </seg>
					<seg type="syl" met="+">es</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">guia </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la<c met="_"> </c>e </seg>
					<seg type="syl" met="-">pu</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">ca.</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Al fin mi giun+se tra l'er-bet+te_e l'ac+que =C -->
					<seg type="syl" met="-">Al </seg>
					<seg type="syl" met="-">fin </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">giun</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="-">l'er</seg>
					<seg type="syl" met="+">bet</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e </seg>
					<seg type="syl" met="+">l'ac</seg>
					<seg type="syl" met="-">que</seg>
				</l>
				<l rhyme="c" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: E mi tra-fis+se_il co+re_e non mi spiac+que, =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">tra</seg>
					<seg type="syl" met="+">fis</seg>
					<seg type="syl" met="-">se<c met="_"> </c>il </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re<c met="_"> </c>e </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">spiac</seg>
					<seg type="syl" met="-">que,</seg>
				</l>
				<l rhyme="d" met="-+---+-" n="settenario">
					<!-- INPUT: Per-ché+ dir non sap-re+i, =d -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">dir </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">sap</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">i,</seg>
				</l>
				<l rhyme="d" met="+-+-++-+-+-" n="endecasillabo">
					<!-- INPUT: S'eb+bi vi+ta più+ dol+ce_o mor+te_in le+i; =D -->
					<seg type="syl" met="+">S'eb</seg>
					<seg type="syl" met="-">bi </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>o </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te<c met="_"> </c>in </seg>
					<seg type="syl" met="+">le</seg>
					<seg type="syl" met="-">i;</seg>
				</l>
				<l rhyme="c" met="-+---++--+-" n="endecasillabo">
					<!-- INPUT: Ma vi+ta se par-lò,+ mor+te se tac+que. =C -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="+">lò, </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="+">tac</seg>
					<seg type="syl" met="-">que.</seg>
				</l>
			</lg>
		</body>
	</text>
