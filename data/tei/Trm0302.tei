<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Siepe, che gli orti vaghi</title>
			</titleStmt>
			<publicationStmt>
				<publisher>Tasso in Music Project</publisher>
				<idno>Trm0302</idno>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Sie+pe, che gli_or+ti va+ghi =a -->
					<seg type="syl" met="+">Sie</seg>
					<seg type="syl" met="-">pe, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>or</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ghi</seg>
				</l>
				<l rhyme="b" met="-----+-" n="settenario">
					<!-- INPUT: E me da me di-vi+di, =b -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">di,</seg>
				</l>
				<l rhyme="b" met="++-+---+-+-" n="endecasillabo">
					<!-- INPUT: Sì+ bel+la ro+sa_in te giam-mai+ non vi+di =B -->
					<seg type="syl" met="+">Sì </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>in </seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">giam</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">di</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Co-m'è+ la don+na mi+a =c+ -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">m'è </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">a</seg>
				</l>
				<l rhyme="c" met="+--+-+-" n="settenario">
					<!-- INPUT: Bel+la,_a-mo-ro+sa_e pi+a; =c -->
					<seg type="syl" met="+">Bel</seg>
					<seg type="syl" met="-">la,<c met="_"> </c>a</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>e </seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">a;</seg>
				</l>
				<l rhyme="d" met="--++-+---+-" n="endecasillabo">
					<!-- INPUT: E men-tr'io+ sten+do sov+ra te la ma+no =D -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">men</seg>
					<seg type="syl" met="+">tr'io </seg>
					<seg type="syl" met="+">sten</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="+">sov</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="d" met="--+--+-" n="settenario">
					<!-- INPUT: La mi strin+ge pian pia+no. =d -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">strin</seg>
					<seg type="syl" met="-">ge </seg>
					<seg type="syl" met="-">pian </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
