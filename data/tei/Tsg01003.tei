<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="---+-++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Sai che là cor+re_il mon+do,_o+ve più ver+si =A+ -->
					<seg type="syl" met="-">Sai </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">là </seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">re<c met="_"> </c>il </seg>
					<seg type="syl" met="+">mon</seg>
					<seg type="syl" met="+">do,<c met="_"> </c>o</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">si</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Di sue dol-cez+ze_il lu-sin-ghier+ Par-na+so; =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">sue </seg>
					<seg type="syl" met="-">dol</seg>
					<seg type="syl" met="+">cez</seg>
					<seg type="syl" met="-">ze<c met="_"> </c>il </seg>
					<seg type="syl" met="-">lu</seg>
					<seg type="syl" met="-">sin</seg>
					<seg type="syl" met="+">ghier </seg>
					<seg type="syl" met="-">Par</seg>
					<seg type="syl" met="+">na</seg>
					<seg type="syl" met="-">so;</seg>
				</l>
				<l rhyme="a" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: E che_’l ve+ro con-di+to_in mol+li ver+si, =A -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">che<c met="_"> </c>’l </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">to<c met="_"> </c>in </seg>
					<seg type="syl" met="+">mol</seg>
					<seg type="syl" met="-">li </seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">si,</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: I più schi+vi_al-let-tan+do_ha per-su-a+so: =B -->
					<seg type="syl" met="-">I </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">schi</seg>
					<seg type="syl" met="-">vi<c met="_"> </c>al</seg>
					<seg type="syl" met="-">let</seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>ha </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="-">su</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">so:</seg>
				</l>
				<l rhyme="a" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Co-sì_a-l’e+gro fan-ciul+ por-gia+mo_a-sper+si =A -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="-">sì<c met="_"> </c>a</seg>
					<seg type="syl" met="+">l’e</seg>
					<seg type="syl" met="-">gro </seg>
					<seg type="syl" met="-">fan</seg>
					<seg type="syl" met="+">ciul </seg>
					<seg type="syl" met="-">por</seg>
					<seg type="syl" met="+">gia</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>a</seg>
					<seg type="syl" met="+">sper</seg>
					<seg type="syl" met="-">si</seg>
				</l>
				<l rhyme="b" met="--+--++--+-" n="endecasillabo">
					<!-- INPUT: Di so-a+vi li-cor+ gli_or+li del va+so; =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="+">cor </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>or</seg>
					<seg type="syl" met="-">li </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">so;</seg>
				</l>
				<l rhyme="c" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Suc+chi_a-ma+ri_in-gan-na+to_in-tan+to_ei be+ve, =C -->
					<seg type="syl" met="+">Suc</seg>
					<seg type="syl" met="-">chi<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>in</seg>
					<seg type="syl" met="-">gan</seg>
					<seg type="syl" met="+">na</seg>
					<seg type="syl" met="-">to<c met="_"> </c>in</seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to<c met="_"> </c>ei </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="-">ve,</seg>
				</l>
				<l rhyme="c" met="---+--+--+-" n="endecasillabo">
					<!-- INPUT: E dal-l’in-gan+no suo vi+ta ri-ce+ve. =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">dal</seg>
					<seg type="syl" met="-">l’in</seg>
					<seg type="syl" met="+">gan</seg>
					<seg type="syl" met="-">no </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">ve.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
