	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: O bel+la de+stra, che_’l so-a+ve pe+gno =A -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">stra, </seg>
					<seg type="syl" met="-">che<c met="_"> </c>’l </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">gno</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: D’a-mi-ci+zia_e di pa+ce_a me por-ge+sti; =B -->
					<seg type="syl" met="-">D’a</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">zia<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>a </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">por</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">sti;</seg>
				</l>
				<l rhyme="a" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: Qua+li_or, las+so, vi tro+vo?_e qual ne ve+gno? =A -->
					<seg type="syl" met="+">Qua</seg>
					<seg type="syl" met="-">li<c met="_"> </c>or, </seg>
					<seg type="syl" met="+">las</seg>
					<seg type="syl" met="-">so, </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">tro</seg>
					<seg type="syl" met="-">vo?<c met="_"> </c>e </seg>
					<seg type="syl" met="-">qual </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">gno?</seg>
				</l>
				<l rhyme="b" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: E voi leg-gia+dre mem+bra,_or non son que+sti =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">leg</seg>
					<seg type="syl" met="+">gia</seg>
					<seg type="syl" met="-">dre </seg>
					<seg type="syl" met="+">mem</seg>
					<seg type="syl" met="-">bra,<c met="_"> </c>or </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sti</seg>
				</l>
				<l rhyme="a" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Del mio fe-ri+no_e scel-le-ra+to sde+gno =A+ -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">no<c met="_"> </c>e </seg>
					<seg type="syl" met="-">scel</seg>
					<seg type="syl" met="-">le</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno</seg>
				</l>
				<l rhyme="b" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: Ve-sti+gi mi-se-ra+bi-li_e fu-ne+sti? =B -->
					<seg type="syl" met="-">Ve</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">bi</seg>
					<seg type="syl" met="-">li<c met="_"> </c>e </seg>
					<seg type="syl" met="-">fu</seg>
					<seg type="syl" met="+">ne</seg>
					<seg type="syl" met="-">sti?</seg>
				</l>
				<l rhyme="c" met="------+--+-" n="endecasillabo">
					<!-- INPUT: O, di par con la man, lu+ci spie-ta+te! =C -->
					<seg type="syl" met="-">O, </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">par </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">man, </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="-">spie</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">te!</seg>
				</l>
				<l rhyme="c" met="+--+-----+-" n="endecasillabo">
					<!-- INPUT: Es+sa le pia+ghe fè, voi le mi-ra+te. =C -->
					<seg type="syl" met="+">Es</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ghe </seg>
					<seg type="syl" met="-">fè, </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
			</lg>
		</body>
	</text>
