<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: U-sci+va_o-mai+ dal mol+le_e fre+sco grem+bo =A+ -->
					<seg type="syl" met="-">U</seg>
					<seg type="syl" met="+">sci</seg>
					<seg type="syl" met="-">va<c met="_"> </c>o</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">dal </seg>
					<seg type="syl" met="+">mol</seg>
					<seg type="syl" met="-">le<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">sco </seg>
					<seg type="syl" met="+">grem</seg>
					<seg type="syl" met="-">bo</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Del+la gran ma+dre sua la not+te_o-scu+ra; =B -->
					<seg type="syl" met="+">Del</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">gran </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">dre </seg>
					<seg type="syl" met="-">sua </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">not</seg>
					<seg type="syl" met="-">te<c met="_"> </c>o</seg>
					<seg type="syl" met="+">scu</seg>
					<seg type="syl" met="-">ra;</seg>
				</l>
				<l rhyme="a" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Au+re lie+vi por-tan+do,_e lar+go nem+bo =A -->
					<seg type="syl" met="+">Au</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">por</seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">do,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">lar</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="+">nem</seg>
					<seg type="syl" met="-">bo</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Di sua ru-gia+da pre-zi-o+sa_e pu+ra: =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">sua </seg>
					<seg type="syl" met="-">ru</seg>
					<seg type="syl" met="+">gia</seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">pre</seg>
					<seg type="syl" met="-">zi</seg>
					<seg type="syl" met="+">o</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>e </seg>
					<seg type="syl" met="+">pu</seg>
					<seg type="syl" met="-">ra:</seg>
				</l>
				<l rhyme="a" met="--+---+--+-" n="endecasillabo">
					<!-- INPUT: E scuo-ten+do del vel l’u+mi-do lem+bo =A -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">scuo</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">vel </seg>
					<seg type="syl" met="+">l’u</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="+">lem</seg>
					<seg type="syl" met="-">bo</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Ne spar-ge+va_i fio-ret+ti_e la ver-du+ra: =B -->
					<seg type="syl" met="-">Ne </seg>
					<seg type="syl" met="-">spar</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">va<c met="_"> </c>i </seg>
					<seg type="syl" met="-">fio</seg>
					<seg type="syl" met="+">ret</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">du</seg>
					<seg type="syl" met="-">ra:</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E_i ven-ti-cel+li, di-bat-ten+do l’a+li, =C+ -->
					<seg type="syl" met="-">E<c met="_"> </c>i </seg>
					<seg type="syl" met="-">ven</seg>
					<seg type="syl" met="-">ti</seg>
					<seg type="syl" met="+">cel</seg>
					<seg type="syl" met="-">li, </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="-">bat</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="+">l’a</seg>
					<seg type="syl" met="-">li,</seg>
				</l>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Lu-sin-ga+va-no_il son+no de’ mor-ta+li. =C -->
					<seg type="syl" met="-">Lu</seg>
					<seg type="syl" met="-">sin</seg>
					<seg type="syl" met="+">ga</seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="-">no<c met="_"> </c>il </seg>
					<seg type="syl" met="+">son</seg>
					<seg type="syl" met="-">no </seg>
					<seg type="syl" met="-">de’ </seg>
					<seg type="syl" met="-">mor</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">li.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
