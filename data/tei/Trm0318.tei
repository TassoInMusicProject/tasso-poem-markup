					<seg type="syl" met="-">so: </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="a" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Per-ché,+ di-ce+va,_an-ch'i+o =a+ -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché, </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">va,<c met="_"> </c>an</seg>
					<seg type="syl" met="+">ch'i</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario">
					<!-- INPUT: Non mi con-ver+to_in fio+re, =b -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">to<c met="_"> </c>in </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="-+-+-+---+-" n="endecasillabo">
					<!-- INPUT: Ben-ché+ non a+mi co+me fe' Nar-ci+so, =C -->
					<seg type="syl" met="-">Ben</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">a</seg>
