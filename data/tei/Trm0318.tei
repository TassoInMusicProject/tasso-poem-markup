	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Sov+ra_un lu+ci-do ri+o =a -->
					<seg type="syl" met="+">Sov</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>un </seg>
					<seg type="syl" met="+">lu</seg>
