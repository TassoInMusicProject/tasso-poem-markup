				</l>
				<l rhyme="b" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Si do-lea+ per a-mo+re =b+ -->
					<seg type="syl" met="-">Si </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">lea </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="c" met="---+-+---+--" n="12">
					<!-- INPUT: Un pa-sto-rel+ mi-ran+do_il suo bel vi+so: &mdash; =C -->
					<seg type="syl" met="-">Un </seg>
					<seg type="syl" met="-">pa</seg>
