	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: La bel+la par-go-let+ta, =a+ -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="-">go</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="b" met="-+-+-+-" n="settenario">
					<!-- INPUT: Ch'an-cor+ non sen+te_a-mo+re, =b -->
					<seg type="syl" met="-">Ch'an</seg>
					<seg type="syl" met="+">cor </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">sen</seg>
					<seg type="syl" met="-">te<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="b" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: Né+ pur no+to_ha per fa+ma_il suo va-lo+re, =B -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">to<c met="_"> </c>ha </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="+">fa</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>il </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="a" met="-++--+-" n="settenario">
					<!-- INPUT: Co' be+gli_oc+chi sa-et+ta =a -->
					<seg type="syl" met="-">Co' </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">et</seg>
					<seg type="syl" met="-">ta</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: E col so-a+ve ri+so, =c -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">col </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">so,</seg>
				</l>
				<l rhyme="c" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: Né+ s'ac-cor+ge che l'ar+me_ha nel bel vi+so. =C -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="-">s'ac</seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">ge </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">l'ar</seg>
					<seg type="syl" met="-">me<c met="_"> </c>ha </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">so.</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario">
					<!-- INPUT: Qual col+pa_ha nel mo-ri+re =b -->
					<seg type="syl" met="-">Qual </seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">pa<c met="_"> </c>ha </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario">
					<!-- INPUT: De la tra-fit+ta gen+te, =d -->
					<seg type="syl" met="-">De </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">tra</seg>
					<seg type="syl" met="+">fit</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="+">gen</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="b" met="-----+-" n="settenario">
					<!-- INPUT: Se non sa di fe-ri+re? =b -->
					<seg type="syl" met="-">Se </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">re?</seg>
				</l>
				<l rhyme="d" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: O bel-lez+za_o-mi-ci+da_ed in-no-cen+te, =D -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">bel</seg>
					<seg type="syl" met="+">lez</seg>
					<seg type="syl" met="-">za<c met="_"> </c>o</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">da<c met="_"> </c>ed </seg>
					<seg type="syl" met="-">in</seg>
					<seg type="syl" met="-">no</seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="e" met="++-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Tem+po_è+ ch'A-mor+ ti mo+stri =e+ -->
					<seg type="syl" met="+">Tem</seg>
					<seg type="syl" met="+">po<c met="_"> </c>è </seg>
					<seg type="syl" met="-">ch'A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">stri</seg>
				</l>
				<l rhyme="e" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: O-mai+ ne le tue pia+ghe_i do+lor no+stri. =E -->
					<seg type="syl" met="-">O</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">tue </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ghe<c met="_"> </c>i </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">lor </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stri.</seg>
				</l>
			</lg>
		</body>
	</text>
