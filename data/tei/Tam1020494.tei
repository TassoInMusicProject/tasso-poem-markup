	<text>
		<body>
			<lg>
				<l rhyme="a" met="++--++-" n="settenario" enjamb="yes">
					<!-- INPUT: Né+ l'a+pi d'al-cun+ fio+re =a+ -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="+">l'a</seg>
					<seg type="syl" met="-">pi </seg>
					<seg type="syl" met="-">d'al</seg>
					<seg type="syl" met="+">cun </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="b" met="+-++-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Cò+glion sì+ dol+ce_il su+co =b+ -->
					<seg type="syl" met="+">Cò</seg>
					<seg type="syl" met="-">glion </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>il </seg>
					<seg type="syl" met="+">su</seg>
					<seg type="syl" met="-">co</seg>
				</l>
				<l rhyme="c" met="+--+-+--++-" n="endecasillabo">
					<!-- INPUT: Co+me fu dol+ce_il me+le ch'al-l'hor+ col+si =C -->
					<seg type="syl" met="+">Co</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>il </seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">ch'al</seg>
					<seg type="syl" met="+">l'hor </seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">si</seg>
				</l>
				<l rhyme="d" met="-+-+-+-" n="settenario">
					<!-- INPUT: Da quel+le fre+sche ro+se, =d -->
					<seg type="syl" met="-">Da </seg>
					<seg type="syl" met="+">quel</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">sche </seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">se,</seg>
				</l>
				<l rhyme="e" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Se ben gli_ar-den+ti ba+ci, =e+ -->
					<seg type="syl" met="-">Se </seg>
					<seg type="syl" met="-">ben </seg>
					<seg type="syl" met="-">gli<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">ba</seg>
					<seg type="syl" met="-">ci,</seg>
				</l>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Che spin-ge+va_il de-si+re_a_i-nhu-mi-dir+si, =C -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">spin</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">va<c met="_"> </c>il </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">re<c met="_"> </c>a<c met="_"> </c>i</seg>
					<seg type="syl" met="-">nhu</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">dir</seg>
					<seg type="syl" met="-">si,</seg>
				</l>
				<l rhyme="f" met="--+--+-" n="settenario">
					<!-- INPUT: Ra-fre-nò+ la te-men+za =f -->
					<seg type="syl" met="-">Ra</seg>
					<seg type="syl" met="-">fre</seg>
					<seg type="syl" met="+">nò </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">te</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">za</seg>
				</l>
				<l rhyme="g" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: E la ver-go+gna,_e fel+li =g+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">go</seg>
					<seg type="syl" met="-">gna,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fel</seg>
					<seg type="syl" met="-">li</seg>
				</l>
				<l rhyme="e" met="++-+-+-" n="settenario">
					<!-- INPUT: Più+ len+ti_e me+no_au-da+ci. =e -->
					<seg type="syl" met="+">Più </seg>
					<seg type="syl" met="+">len</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e </seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">no<c met="_"> </c>au</seg>
					<seg type="syl" met="+">da</seg>
					<seg type="syl" met="-">ci.</seg>
				</l>
			</lg>
		</body>
	</text>
