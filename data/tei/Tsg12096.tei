<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--++--+-+-" n="endecasillabo">
					<!-- INPUT: Giun+to_a la tom+ba_o+ve_al suo spir+to vi+vo =A -->
					<seg type="syl" met="+">Giun</seg>
					<seg type="syl" met="-">to<c met="_"> </c>a </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">tom</seg>
					<seg type="syl" met="+">ba<c met="_"> </c>o</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>al </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="+">spir</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">vo</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Do-lo-ro+sa pri-gio+ne_il Ciel pre-scris+se; =B -->
					<seg type="syl" met="-">Do</seg>
					<seg type="syl" met="-">lo</seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">pri</seg>
					<seg type="syl" met="+">gio</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>il </seg>
					<seg type="syl" met="-">Ciel </seg>
					<seg type="syl" met="-">pre</seg>
					<seg type="syl" met="+">scris</seg>
					<seg type="syl" met="-">se;</seg>
				</l>
				<l rhyme="a" met="+--+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Pal+li-do, fred+do, mu+to,_e qua+si pri+vo =A+ -->
					<seg type="syl" met="+">Pal</seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="-">do, </seg>
					<seg type="syl" met="+">fred</seg>
					<seg type="syl" met="-">do, </seg>
					<seg type="syl" met="+">mu</seg>
					<seg type="syl" met="-">to,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">qua</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">vo</seg>
				</l>
				<l rhyme="b" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Di mo-vi-men+to,_al mar+mo gli_oc+chi_af-fis+se. =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">to,<c met="_"> </c>al </seg>
					<seg type="syl" met="+">mar</seg>
					<seg type="syl" met="-">mo </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fis</seg>
					<seg type="syl" met="-">se.</seg>
				</l>
				<l rhyme="a" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Al-fin+ sgor-gan+do_un la-gri-mo+so ri+vo, =A -->
					<seg type="syl" met="-">Al</seg>
					<seg type="syl" met="+">fin </seg>
					<seg type="syl" met="-">sgor</seg>
					<seg type="syl" met="+">gan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>un </seg>
					<seg type="syl" met="-">la</seg>
					<seg type="syl" met="-">gri</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">so </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">vo,</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: In un lan+gui-do_oi-mè+ pro-rup+pe,_e dis+se: =B -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="-">un </seg>
					<seg type="syl" met="+">lan</seg>
					<seg type="syl" met="-">gui</seg>
					<seg type="syl" met="-">do<c met="_"> </c>oi</seg>
					<seg type="syl" met="+">mè </seg>
					<seg type="syl" met="-">pro</seg>
					<seg type="syl" met="+">rup</seg>
					<seg type="syl" met="-">pe,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">dis</seg>
					<seg type="syl" met="-">se:</seg>
				</l>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: "O sas+so_a-ma+to_ed o-no-ra+to tan+to =C -->
					<seg type="syl" met="-">"O </seg>
					<seg type="syl" met="+">sas</seg>
					<seg type="syl" met="-">so<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">to<c met="_"> </c>ed </seg>
					<seg type="syl" met="-">o</seg>
					<seg type="syl" met="-">no</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="c" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Che den+tro_hai le mie fiam+me,_e fuo+ri_il pian+to:" =C -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">tro<c met="_"> </c>hai </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">mie </seg>
					<seg type="syl" met="+">fiam</seg>
					<seg type="syl" met="-">me,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fuo</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>il </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">to:"</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
