<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---++--+-" n="endecasillabo">
					<!-- INPUT: Gia-cea+ la mia vir-tù+ vin+ta_e smar-ri+ta =A -->
					<seg type="syl" met="-">Gia</seg>
					<seg type="syl" met="+">cea </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">vir</seg>
					<seg type="syl" met="+">tù </seg>
					<seg type="syl" met="+">vin</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>e </seg>
					<seg type="syl" met="-">smar</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">ta</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Nel duol, ch'è sem+pre_in sua ra-gion+ più for+te, =B -->
					<seg type="syl" met="-">Nel </seg>
					<seg type="syl" met="-">duol, </seg>
					<seg type="syl" met="-">ch'è </seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pre<c met="_"> </c>in </seg>
					<seg type="syl" met="-">sua </seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">gion </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Quan+do pie-to+sa di sì du+ra sor+te =B -->
					<seg type="syl" met="+">Quan</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">sì </seg>
					<seg type="syl" met="+">du</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="+">sor</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="a" met="-++--+-+-+-" n="endecasillabo">
					<!-- INPUT: Ven-ne_in+ so+gno Ma-don+na_a dar+le_a-i+ta; =A -->
					<seg type="syl" met="-">Ven</seg>
					<seg type="syl" met="+">ne<c met="_"> </c>in </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="-">Ma</seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na<c met="_"> </c>a </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">le<c met="_"> </c>a</seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">ta;</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="a" met="---+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E ri-sto-rò+ gli spir+ti,_e_'n me so-pi+ta =A+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">sto</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="-">gli </seg>
					<seg type="syl" met="+">spir</seg>
					<seg type="syl" met="-">ti,<c met="_"> </c>e<c met="_"> </c>'n </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">ta</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: La do+glia_a no+va spe+me_ap-rì+ le por+te: =B -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">glia<c met="_"> </c>a </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">va </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">me<c met="_"> </c>ap</seg>
					<seg type="syl" met="+">rì </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">por</seg>
					<seg type="syl" met="-">te:</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: E co-sì+ ne l'i-ma+gi-ne di mor+te =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">l'i</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">gi</seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="a" met="-++----+-+-" n="endecasillabo">
					<!-- INPUT: Tro-vò+ l'e+gro mio cor sa-lu+te_e vi+ta. =A -->
					<seg type="syl" met="-">Tro</seg>
					<seg type="syl" met="+">vò </seg>
					<seg type="syl" met="+">l'e</seg>
					<seg type="syl" met="-">gro </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: El+la, vol-gen+do gli_oc+chi_in dol+ci gi+ri, =c -->
					<seg type="syl" met="+">El</seg>
					<seg type="syl" met="-">la, </seg>
					<seg type="syl" met="-">vol</seg>
					<seg type="syl" met="+">gen</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi<c met="_"> </c>in </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">gi</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="d" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: Pa-rea+ che mi di-ces+se:_&mdash;_a che pur tan+to, =D -->
					<seg type="syl" met="-">Pa</seg>
					<seg type="syl" met="+">rea </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">ces</seg>
					<seg type="syl" met="-">se:<c met="_"> </c>&amp;mdash;<c met="_"> </c>a </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="e" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: O mio fe-del,+ t'af-flig+gi_e ti con-su+mi? =E -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">del, </seg>
					<seg type="syl" met="-">t'af</seg>
					<seg type="syl" met="+">flig</seg>
					<seg type="syl" met="-">gi<c met="_"> </c>e </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">su</seg>
					<seg type="syl" met="-">mi?</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: E per-ché+ non fai tre+gua_a' tuoi sos-pi+ri, =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">fai </seg>
					<seg type="syl" met="+">tre</seg>
					<seg type="syl" met="-">gua<c met="_"> </c>a' </seg>
					<seg type="syl" met="-">tuoi </seg>
					<seg type="syl" met="-">sos</seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="d" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: E_'n que+st'a-ma+te lu+ci_a-sciu+ghi_il pian+to? =D -->
					<seg type="syl" met="-">E<c met="_"> </c>'n </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">st'a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>a</seg>
					<seg type="syl" met="+">sciu</seg>
					<seg type="syl" met="-">ghi<c met="_"> </c>il </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">to?</seg>
				</l>
				<l rhyme="e" met="+-+--+-+-+--" n="12">
					<!-- INPUT: Spe+ri for+se d'a-ver+ più fi+di lu+mi? &mdash; =E -->
					<seg type="syl" met="+">Spe</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">d'a</seg>
					<seg type="syl" met="+">ver </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">mi? </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
