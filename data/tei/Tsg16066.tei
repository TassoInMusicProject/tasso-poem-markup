	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Que+sta bel-lez+za mia sa-rà+ mer-ce+de =A -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="-">bel</seg>
					<seg type="syl" met="+">lez</seg>
					<seg type="syl" met="-">za </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="-">mer</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">de</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Del tron-ca-tor+ del-l’e-se-cra+bil te+sta. =B -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="-">tron</seg>
					<seg type="syl" met="-">ca</seg>
