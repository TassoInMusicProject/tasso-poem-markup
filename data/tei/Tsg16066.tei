					<seg type="syl" met="-">bil </seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">sta.</seg>
				</l>
				<l rhyme="a" met="---+-++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: O miei fa-mo+si_a-man+ti,_ec+co si chie+de =A+ -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">miei </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">si<c met="_"> </c>a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="+">ti,<c met="_"> </c>ec</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">chie</seg>
					<seg type="syl" met="-">de</seg>
				</l>
				<l rhyme="b" met="-+-----+-+-" n="endecasillabo">
					<!-- INPUT: Dif-fi+cil sì, da voi, ma_im-pre+sa_o-ne+sta. =B -->
					<seg type="syl" met="-">Dif</seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">cil </seg>
					<seg type="syl" met="-">sì, </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">voi, </seg>
					<seg type="syl" met="-">ma<c met="_"> </c>im</seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>o</seg>
					<seg type="syl" met="+">ne</seg>
					<seg type="syl" met="-">sta.</seg>
				</l>
				<l rhyme="a" met="---++--+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Io che sa-rò+ d’am+pie ric-chez+ze_e-re+de, =A+ -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="+">d’am</seg>
					<seg type="syl" met="-">pie </seg>
					<seg type="syl" met="-">ric</seg>
					<seg type="syl" met="+">chez</seg>
					<seg type="syl" met="-">ze<c met="_"> </c>e</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">de,</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: D’u+na ven-det+ta_in gui-der-don+ son pre+sta. =B -->
					<seg type="syl" met="+">D’u</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">ven</seg>
					<seg type="syl" met="+">det</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>in </seg>
