					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">sta.</seg>
				</l>
				<l rhyme="c" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: S’es+ser com+pra_a tal prez+zo_in-de+gna_io so+no, =C -->
					<seg type="syl" met="+">S’es</seg>
					<seg type="syl" met="-">ser </seg>
					<seg type="syl" met="+">com</seg>
					<seg type="syl" met="-">pra<c met="_"> </c>a </seg>
					<seg type="syl" met="-">tal </seg>
					<seg type="syl" met="+">prez</seg>
					<seg type="syl" met="-">zo<c met="_"> </c>in</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gna<c met="_"> </c>io </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="c" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Bel-tà,+ sei di na-tu+ra_i-nu+til do+no. =C -->
					<seg type="syl" met="-">Bel</seg>
					<seg type="syl" met="+">tà, </seg>
					<seg type="syl" met="-">sei </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">na</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>i</seg>
					<seg type="syl" met="+">nu</seg>
					<seg type="syl" met="-">til </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
