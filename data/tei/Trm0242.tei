	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Sov+ra l'er-bet+te_e_i fio+ri =a -->
					<seg type="syl" met="+">Sov</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">l'er</seg>
					<seg type="syl" met="+">bet</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Fug+gia tut+to smar-ri+to =b+ -->
					<seg type="syl" met="+">Fug</seg>
					<seg type="syl" met="-">gia </seg>
					<seg type="syl" met="+">tut</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">smar</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="a" met="---+-+-" n="settenario">
					<!-- INPUT: La mia cru-del+ Li-co+ri, =a -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">del </seg>
					<seg type="syl" met="-">Li</seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="b" met="+--------+-" n="endecasillabo">
					<!-- INPUT: An+zi_'l cor mio che fu da lei ra-pi+to; =B -->
					<seg type="syl" met="+">An</seg>
					<seg type="syl" met="-">zi<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">lei </seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">to;</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: E me di piag+gia_in piag+gia =c -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">piag</seg>
					<seg type="syl" met="-">gia<c met="_"> </c>in </seg>
					<seg type="syl" met="+">piag</seg>
					<seg type="syl" met="-">gia</seg>
				</l>
				<l rhyme="c" met="-++--+-" n="settenario">
					<!-- INPUT: Se-guìa+ Nin+fa sel-vag+gia, =c -->
					<seg type="syl" met="-">Se</seg>
					<seg type="syl" met="+">guìa </seg>
					<seg type="syl" met="+">Nin</seg>
					<seg type="syl" met="-">fa </seg>
					<seg type="syl" met="-">sel</seg>
					<seg type="syl" met="+">vag</seg>
					<seg type="syl" met="-">gia,</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Quan+do m'ag-giun+se,_e con so-a+vi ba+ci =D -->
					<seg type="syl" met="+">Quan</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">m'ag</seg>
					<seg type="syl" met="+">giun</seg>
					<seg type="syl" met="-">se,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">ba</seg>
					<seg type="syl" met="-">ci</seg>
				</l>
				<l rhyme="d" met="-+-+-+--" n="8">
					<!-- INPUT: Mi dis+se:_&mdash;_or pren+di_e ta+ci. &mdash; =d -->
					<seg type="syl" met="-">Mi </seg>
					<seg type="syl" met="+">dis</seg>
					<seg type="syl" met="-">se:<c met="_"> </c>&amp;mdash;<c met="_"> </c>or </seg>
					<seg type="syl" met="+">pren</seg>
					<seg type="syl" met="-">di<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">ci. </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
			</lg>
		</body>
	</text>
