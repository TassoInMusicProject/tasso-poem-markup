<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-++--+-" n="settenario">
					<!-- INPUT: A-mor+ l'al+ma m'al-lac+cia =a -->
					<seg type="syl" met="-">A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">m'al</seg>
					<seg type="syl" met="+">lac</seg>
					<seg type="syl" met="-">cia</seg>
				</l>
				<l rhyme="b" met="-++--+-" n="settenario">
					<!-- INPUT: Di dol+ci_a+spre- ca-te+ne: =b -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="+">ci<c met="_"> </c>a</seg>
					<seg type="syl" met="-">spre </seg>
					<seg type="syl" met="-">ca</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">ne:</seg>
				</l>
				<l rhyme="c" met="--++-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Non mi do+glio_io+ per ciò,+ ma ben l'ac-cu+so =C+ -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="+">glio<c met="_"> </c>io </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="+">ciò, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">ben </seg>
					<seg type="syl" met="-">l'ac</seg>
					<seg type="syl" met="+">cu</seg>
					<seg type="syl" met="-">so</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Che mi le+ghi_ed af-fre+ne =b+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">le</seg>
					<seg type="syl" met="-">ghi<c met="_"> </c>ed </seg>
					<seg type="syl" met="-">af</seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">ne</seg>
				</l>
				<l rhyme="a" met="-+-+-+-" n="settenario">
					<!-- INPUT: La lin+gua_a ciò+ ch'io tac+cia =a -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">lin</seg>
					<seg type="syl" met="-">gua<c met="_"> </c>a </seg>
					<seg type="syl" met="+">ciò </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="+">tac</seg>
					<seg type="syl" met="-">cia</seg>
				</l>
				<l rhyme="c" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: An+zi_a ma-don+na ti+mi-do_e con-fu+so =C -->
					<seg type="syl" met="+">An</seg>
					<seg type="syl" met="-">zi<c met="_"> </c>a </seg>
					<seg type="syl" met="-">ma</seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="-">do<c met="_"> </c>e </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">fu</seg>
					<seg type="syl" met="-">so</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: E'n mia ra-gion+ de-lu+so. =c -->
					<seg type="syl" met="-">E'n </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">gion </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">so.</seg>
				</l>
				<l rhyme="d" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Scio+gli, pie-to+so_A-mo+re, =d+ -->
					<seg type="syl" met="+">Scio</seg>
					<seg type="syl" met="-">gli, </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">so<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="e" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: La lin+gua,_e se non vuo+i =e+ -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">lin</seg>
					<seg type="syl" met="-">gua,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">vuo</seg>
					<seg type="syl" met="-">i</seg>
				</l>
				<l rhyme="e" met="--+----+-+-" n="endecasillabo">
					<!-- INPUT: Che mi strin+ga_un sol men de' lac+ci tuo+i, =E -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">strin</seg>
					<seg type="syl" met="-">ga<c met="_"> </c>un </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="-">men </seg>
					<seg type="syl" met="-">de' </seg>
					<seg type="syl" met="+">lac</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">tuo</seg>
					<seg type="syl" met="-">i,</seg>
				</l>
				<l rhyme="d" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Tan+ti n'ag-giun+gi_in quel+la ve+ce_al co+re. =D -->
					<seg type="syl" met="+">Tan</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">n'ag</seg>
					<seg type="syl" met="+">giun</seg>
					<seg type="syl" met="-">gi<c met="_"> </c>in </seg>
					<seg type="syl" met="+">quel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>al </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
