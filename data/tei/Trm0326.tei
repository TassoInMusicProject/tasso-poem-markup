<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Lunge da gli occhi vostri</title>
			</titleStmt>
			<publicationStmt>
				<publisher>Tasso in Music Project</publisher>
				<idno>Trm0326</idno>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Lun+ge da gli_oc+chi vo+stri =a -->
					<seg type="syl" met="+">Lun</seg>
					<seg type="syl" met="-">ge </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stri</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Io vi+vo del pen-sie+ro =b+ -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">vo </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">ro</seg>
				</l>
				<l rhyme="b" met="-+-+-+--++-" n="endecasillabo">
					<!-- INPUT: Pen-so+sa vi+ta;_e vi+vo per-ché_i'+ spe+ro. =B -->
					<seg type="syl" met="-">Pen</seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta;<c met="_"> </c>e </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">vo </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ché<c met="_"> </c>i' </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">ro.</seg>
				</l>
				<l rhyme="c" met="+-+--+-" n="settenario">
					<!-- INPUT: Spe+ro_il lie+to ri-tor+no; =c -->
					<seg type="syl" met="+">Spe</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>il </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">tor</seg>
					<seg type="syl" met="-">no;</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E s'av-ver-rà+ che nel fe-li+ce gior+no =C+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">s'av</seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">gior</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="d" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: La mia dol+ce spe-ran+za_in me si mo+ia, =D -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">spe</seg>
					<seg type="syl" met="+">ran</seg>
					<seg type="syl" met="-">za<c met="_"> </c>in </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">ia,</seg>
				</l>
				<l rhyme="d" met="+-+--+-" n="settenario">
					<!-- INPUT: Spe+ro vi+ver di gio+ia. =d -->
					<seg type="syl" met="+">Spe</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ver </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">gio</seg>
					<seg type="syl" met="-">ia.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
