	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Dis-pie-ta+ta pie-ta+de =a+ -->
					<seg type="syl" met="-">Dis</seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">de</seg>
				</l>
				<l rhyme="b" met="-----+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Fu la tua ve-ra-men+te,_o Daf+ne,_al-l'ho+ra =B+ -->
					<seg type="syl" met="-">Fu </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">tua </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>o </seg>
					<seg type="syl" met="+">Daf</seg>
					<seg type="syl" met="-">ne,<c met="_"> </c>al</seg>
					<seg type="syl" met="+">l'ho</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: Che ri-te-ne+sti_il dar+do; =c -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">te</seg>
					<seg type="syl" met="+">ne</seg>
					<seg type="syl" met="-">sti<c met="_"> </c>il </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">do;</seg>
				</l>
				<l rhyme="d" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Pe-rò+ che_'l mio mo-ri+re =d+ -->
					<seg type="syl" met="-">Pe</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="-">che<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="c" met="+-+--++-++-" n="endecasillabo">
					<!-- INPUT: Più+ a-ma+ro sa-rà,+ quan+to più+ tar+do. =C -->
					<seg type="syl" met="+">Più </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rà, </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="+">tar</seg>
					<seg type="syl" met="-">do.</seg>
				</l>
				<l rhyme="e" met="---+-+-" n="settenario">
					<!-- INPUT: Et or per-ché+ m'a-vol+gi =e -->
					<seg type="syl" met="-">Et </seg>
					<seg type="syl" met="-">or </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">m'a</seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">gi</seg>
				</l>
				<l rhyme="f" met="-+-+-+--++-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Per sì+ di-ver+se stra+de_e per sí+ va+ri =F+ -->
					<seg type="syl" met="-">Per </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">de<c met="_"> </c>e </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="+">sí </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="g" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: Ra-gio-na-men+ti_in-va+no? di che te+mi? =G -->
					<seg type="syl" met="-">Ra</seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="-">na</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>in</seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">no? </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">mi?</seg>
				</l>
				<l rhyme="h" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: Ch'io non m'uc-ci+da? Te+mi del mio be+ne. =H -->
					<seg type="syl" met="-">Ch'io </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">m'uc</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">da? </seg>
					<seg type="syl" met="+">Te</seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="-">ne.</seg>
				</l>
			</lg>
		</body>
	</text>
