<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Io veg+gio_in cie+lo scin-til-lar+ le stel+le =A -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="+">veg</seg>
					<seg type="syl" met="-">gio<c met="_"> </c>in </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo </seg>
					<seg type="syl" met="-">scin</seg>
					<seg type="syl" met="-">til</seg>
					<seg type="syl" met="+">lar </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">stel</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Ol+tre l'u-sa+to_e lam-peg-giar+ tre-man+ti, =B -->
					<seg type="syl" met="+">Ol</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">l'u</seg>
					<seg type="syl" met="+">sa</seg>
					<seg type="syl" met="-">to<c met="_"> </c>e </seg>
					<seg type="syl" met="-">lam</seg>
					<seg type="syl" met="-">peg</seg>
					<seg type="syl" met="+">giar </seg>
					<seg type="syl" met="-">tre</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">ti,</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Co+me ne gli_oc+chi de' cor-te+si_a-man+ti =B -->
					<seg type="syl" met="+">Co</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">de' </seg>
					<seg type="syl" met="-">cor</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">si<c met="_"> </c>a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="a" met="---+-++--+-" n="endecasillabo">
					<!-- INPUT: Noi ri-mi-riam+ ta-lor+ vi+ve fa-cel+le. =A -->
					<seg type="syl" met="-">Noi </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">riam </seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="+">lor </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">cel</seg>
					<seg type="syl" met="-">le.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="a" met="-++--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: A-man+ for+se là su+so,_o pur son el+le =A+ -->
					<seg type="syl" met="-">A</seg>
					<seg type="syl" met="+">man </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">là </seg>
					<seg type="syl" met="+">su</seg>
					<seg type="syl" met="-">so,<c met="_"> </c>o </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">el</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Pie-to+se_a' no+stri_af-fan+ni,_a' no+stri pian+ti? =B -->
					<seg type="syl" met="-">Pie</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">se<c met="_"> </c>a' </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stri<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fan</seg>
					<seg type="syl" met="-">ni,<c met="_"> </c>a' </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stri </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">ti?</seg>
				</l>
				<l rhyme="b" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Men+tre scor-gon+ le_in-si+die_e_i pas+si_er-ran+ti =B -->
					<seg type="syl" met="+">Men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">scor</seg>
					<seg type="syl" met="+">gon </seg>
					<seg type="syl" met="-">le<c met="_"> </c>in</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">die<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">pas</seg>
					<seg type="syl" met="-">si<c met="_"> </c>er</seg>
					<seg type="syl" met="+">ran</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="a" met="-++--++--+-" n="endecasillabo">
					<!-- INPUT: Là do+ve_al+tri d'A-mor+ go+da_e fa-vel+le? =A -->
					<seg type="syl" met="-">Là </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="+">ve<c met="_"> </c>al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="-">d'A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="+">go</seg>
					<seg type="syl" met="-">da<c met="_"> </c>e </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">vel</seg>
					<seg type="syl" met="-">le?</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Cor-te+si lu+ci, se Le-an+dro_in ma+re =C -->
					<seg type="syl" met="-">Cor</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ci, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">Le</seg>
					<seg type="syl" met="+">an</seg>
					<seg type="syl" met="-">dro<c met="_"> </c>in </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: O tra-vï-a+to pe-re-grin+ fos-s'i+o =D -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">tra</seg>
					<seg type="syl" met="-">vï</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">pe</seg>
					<seg type="syl" met="-">re</seg>
					<seg type="syl" met="+">grin </seg>
					<seg type="syl" met="-">fos</seg>
					<seg type="syl" met="+">s'i</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Non mi sa-re+ste di soc-cor+so_a-va+re: =C -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">ste </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">soc</seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">so<c met="_"> </c>a</seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">re:</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Co-sì+ vi fac+cia_il Sol più bel+le_e chia+re, =C -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">fac</seg>
					<seg type="syl" met="-">cia<c met="_"> </c>il </seg>
					<seg type="syl" met="-">Sol </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">le<c met="_"> </c>e </seg>
					<seg type="syl" met="+">chia</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="d" met="+--+-+--++-" n="endecasillabo">
					<!-- INPUT: Sia+te nel dub+bio cor+so_al de-sir+ mi+o =D -->
					<seg type="syl" met="+">Sia</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">dub</seg>
					<seg type="syl" met="-">bio </seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">so<c met="_"> </c>al </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">sir </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="c" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Fi+de mie du+ci_e scor+te_a-ma+te_e ca+re. =C -->
					<seg type="syl" met="+">Fi</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">mie </seg>
					<seg type="syl" met="+">du</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>e </seg>
					<seg type="syl" met="+">scor</seg>
					<seg type="syl" met="-">te<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
