	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: D'a+ria_un tem+po nu-drim+mi,_e ci+bo_e vi+ta =A+ -->
					<seg type="syl" met="+">D'a</seg>
					<seg type="syl" met="-">ria<c met="_"> </c>un </seg>
					<seg type="syl" met="+">tem</seg>
					<seg type="syl" met="-">po </seg>
					<seg type="syl" met="-">nu</seg>
					<seg type="syl" met="+">drim</seg>
					<seg type="syl" met="-">mi,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">bo<c met="_"> </c>e </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta</seg>
				</l>
				<l rhyme="b" met="+------+-+-" n="endecasillabo">
					<!-- INPUT: L'au+ra mi fu che d'un bel vol+to spi+ra; =B -->
					<seg type="syl" met="+">L'au</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">d'un </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ra;</seg>
				</l>
				<l rhyme="b" met="-----+-+-+-" n="endecasillabo">
					<!-- INPUT: Or che lei mi con-ten+de_or-go+glio_ed i+ra, =B -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">lei </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">de<c met="_"> </c>or</seg>
					<seg type="syl" met="+">go</seg>
					<seg type="syl" met="-">glio<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">ra,</seg>
				</l>
				<l rhyme="a" met="--+--++--+-" n="endecasillabo">
					<!-- INPUT: Di qual e+sca sa-rà+ l'al+ma nu-dri+ta? =A -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">qual </seg>
					<seg type="syl" met="+">e</seg>
					<seg type="syl" met="-">sca </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">nu</seg>
					<seg type="syl" met="+">dri</seg>
					<seg type="syl" met="-">ta?</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="a" met="--+--+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: I fa-me+li-ci spir+ti_in va+no_a-i+ta =A+ -->
					<seg type="syl" met="-">I </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">spir</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>in </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">no<c met="_"> </c>a</seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">ta</seg>
				</l>
				<l rhyme="b" met="+--+--+--+-" n="endecasillabo">
					<!-- INPUT: Chia+ma-no,_e_'n dar+no_il cor lan+gue_e so-spi+ra; =B -->
					<seg type="syl" met="+">Chia</seg>
					<seg type="syl" met="-">ma</seg>
					<seg type="syl" met="-">no,<c met="_"> </c>e<c met="_"> </c>'n </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">no<c met="_"> </c>il </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="+">lan</seg>
					<seg type="syl" met="-">gue<c met="_"> </c>e </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ra;</seg>
				</l>
				<l rhyme="b" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Ma, se pur l'em+pia_a dar+le mor+te_a-spi+ra =B -->
					<seg type="syl" met="-">Ma, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="+">l'em</seg>
					<seg type="syl" met="-">pia<c met="_"> </c>a </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te<c met="_"> </c>a</seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="a" met="+----+---+-" n="endecasillabo">
					<!-- INPUT: Muo+ia non per di-giun,+ ma per fe-ri+ta. =A -->
					<seg type="syl" met="+">Muo</seg>
					<seg type="syl" met="-">ia </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">giun, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">ta.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Ar+mi gli_oc+chi di sde+gno_e stra+li_av-ven+ti =C -->
					<seg type="syl" met="+">Ar</seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>e </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">li<c met="_"> </c>av</seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="d" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: A mil+le_a mil+le,_a' fe+ri col+pi_i-gnu+da =D -->
					<seg type="syl" met="-">A </seg>
					<seg type="syl" met="+">mil</seg>
					<seg type="syl" met="-">le<c met="_"> </c>a </seg>
					<seg type="syl" met="+">mil</seg>
					<seg type="syl" met="-">le,<c met="_"> </c>a' </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">pi<c met="_"> </c>i</seg>
					<seg type="syl" met="+">gnu</seg>
					<seg type="syl" met="-">da</seg>
				</l>
				<l rhyme="e" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Io por+go l'al+ma non ch'i-ner+me_il se+no. =E -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="+">por</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">ch'i</seg>
					<seg type="syl" met="+">ner</seg>
					<seg type="syl" met="-">me<c met="_"> </c>il </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Fac+cia_il mio stra+tio_i suoi de-sir+ con-ten+ti; =C -->
					<seg type="syl" met="+">Fac</seg>
					<seg type="syl" met="-">cia<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">tio<c met="_"> </c>i </seg>
					<seg type="syl" met="-">suoi </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">sir </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">ti;</seg>
				</l>
				<l rhyme="e" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Ben fia pie-tà,+ ch'io la ri-veg+gia_al-me+no, =E -->
					<seg type="syl" met="-">Ben </seg>
					<seg type="syl" met="-">fia </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">tà, </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">veg</seg>
					<seg type="syl" met="-">gia<c met="_"> </c>al</seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="d" met="-+----+--+-" n="endecasillabo">
					<!-- INPUT: Non di+co pia, ma di-sde+gno-sa_e cru+da. =D -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">pia, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>e </seg>
					<seg type="syl" met="+">cru</seg>
					<seg type="syl" met="-">da.</seg>
				</l>
			</lg>
		</body>
	</text>
