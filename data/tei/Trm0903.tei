					<seg type="syl" met="-">ro<c met="_"> </c>a</seg>
					<seg type="syl" met="+">dor</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario">
					<!-- INPUT: Ma-don+na fa sog-gior+no, =b -->
					<seg type="syl" met="-">Ma</seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">fa </seg>
					<seg type="syl" met="-">sog</seg>
					<seg type="syl" met="+">gior</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="c" met="+--+--++-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Deh!+ non t'in-cre+sca, ch'in sì+ ca+ro lo+co =C+ -->
					<seg type="syl" met="+">Deh! </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">t'in</seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">sca, </seg>
					<seg type="syl" met="-">ch'in </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">co</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario">
					<!-- INPUT: Av-rei+ la mor+te_a gio+co; =c -->
					<seg type="syl" met="-">Av</seg>
					<seg type="syl" met="+">rei </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te<c met="_"> </c>a </seg>
					<seg type="syl" met="+">gio</seg>
					<seg type="syl" met="-">co;</seg>
				</l>
				<l rhyme="d" met="-+++-++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Per-ché+ do+v'el+la cac+cia,_oh!+ pur me_'l cre+da, =D+ -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="+">v'el</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">cac</seg>
					<seg type="syl" met="+">cia,<c met="_"> </c>oh! </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">me<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">da,</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Es+ser io bra+mo_o pre-da-to+re_o pre+da. =D -->
					<seg type="syl" met="+">Es</seg>
					<seg type="syl" met="-">ser </seg>
					<seg type="syl" met="-">io </seg>
					<seg type="syl" met="+">bra</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>o </seg>
					<seg type="syl" met="-">pre</seg>
					<seg type="syl" met="-">da</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">re<c met="_"> </c>o </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">da.</seg>
