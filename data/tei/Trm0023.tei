	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario">
					<!-- INPUT: Io non pos+so gio-i+re =a -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">pos</seg>
					<seg type="syl" met="-">so </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="a" met="+----+---+-" n="endecasillabo">
					<!-- INPUT: Lun+ge da voi, che sie+te_il mio de-si+re; =A -->
					<seg type="syl" met="+">Lun</seg>
					<seg type="syl" met="-">ge </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">voi, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">re;</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ma_'l mio pen-sier+ fal-la+ce =b+ -->
					<seg type="syl" met="-">Ma<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sier </seg>
					<seg type="syl" met="-">fal</seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">ce</seg>
				</l>
				<l rhyme="c" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Pas+sa mon+ti_e cam-pa+gne_e ma+ri_e fiu+mi; =C -->
					<seg type="syl" met="+">Pas</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="+">mon</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e </seg>
					<seg type="syl" met="-">cam</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">gne<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fiu</seg>
					<seg type="syl" met="-">mi;</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario">
					<!-- INPUT: E m'a-vi-ci+na_e sfa+ce =b -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">m'a</seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">na<c met="_"> </c>e </seg>
					<seg type="syl" met="+">sfa</seg>
					<seg type="syl" met="-">ce</seg>
				</l>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Al dol+ce fo+co de' be' vo+stri lu+mi; =C -->
					<seg type="syl" met="-">Al </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">de' </seg>
					<seg type="syl" met="-">be' </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stri </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">mi;</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: E_'l lan-guir+ sì mi pia+ce, =b -->
					<seg type="syl" met="-">E<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">lan</seg>
					<seg type="syl" met="+">guir </seg>
					<seg type="syl" met="-">sì </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ce,</seg>
				</l>
				<l rhyme="a" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Ch'in-fi-ni+to di-let+to_ho nel mar-ti+re. =A -->
					<seg type="syl" met="-">Ch'in</seg>
					<seg type="syl" met="-">fi</seg>
					<seg type="syl" met="+">ni</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">to<c met="_"> </c>ho </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">mar</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
			</lg>
