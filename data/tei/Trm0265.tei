	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Men+tre la don+na mia can-gian+do_a-spet+to =A+ -->
					<seg type="syl" met="+">Men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">can</seg>
					<seg type="syl" met="+">gian</seg>
					<seg type="syl" met="-">do<c met="_"> </c>a</seg>
					<seg type="syl" met="+">spet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Di bian+co nel ver-mi+glio =b+ -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">bian</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">glio</seg>
				</l>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Mo+stra l'in-ter+no_af-fet+to =a -->
					<seg type="syl" met="+">Mo</seg>
					<seg type="syl" met="-">stra </seg>
					<seg type="syl" met="-">l'in</seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">no<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: E par or fre+sca ro+sa_or va+go gi+glio, =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">par </seg>
					<seg type="syl" met="-">or </seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">sca </seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>or </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="+">gi</seg>
					<seg type="syl" met="-">glio,</seg>
				</l>
				<l rhyme="c" met="+----++---+-" n="12">
					<!-- INPUT: Di+co &mdash; se co-sì+ mu+ta_il bel co-lo+re, =C -->
					<seg type="syl" met="+">Di</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">mu</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>il </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="-++--+-" n="settenario">
					<!-- INPUT: Non è+ fer+ma nel co+re_&mdash;_: =c -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="+">è </seg>
					<seg type="syl" met="+">fer</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re<c met="_"> </c>&amp;mdash;<c met="_"> </c>:</seg>
				</l>
				<l rhyme="d" met="---++-++-+-" n="endecasillabo">
					<!-- INPUT: Ma_'l va-rï-ar+ è+ co-sì+ dol+ce_e va+go =D -->
					<seg type="syl" met="-">Ma<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="-">rï</seg>
					<seg type="syl" met="+">ar </seg>
					<seg type="syl" met="+">è </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>e </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">go</seg>
				</l>
				<l rhyme="d" met="-+---+-" n="settenario">
					<!-- INPUT: Che d'al+tro_io non m'ap-pa+go. =d -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">d'al</seg>
					<seg type="syl" met="-">tro<c met="_"> </c>io </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">m'ap</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">go.</seg>
				</l>
