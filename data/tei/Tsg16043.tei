<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Qual mu+si-co gen-til,+ pri+ma che chia+ra =A+ -->
					<seg type="syl" met="-">Qual </seg>
					<seg type="syl" met="+">mu</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="-">gen</seg>
					<seg type="syl" met="+">til, </seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">chia</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Al-ta-men+te la lin+gua_al can+to sno+di; =B -->
					<seg type="syl" met="-">Al</seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">lin</seg>
					<seg type="syl" met="-">gua<c met="_"> </c>al </seg>
					<seg type="syl" met="+">can</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">sno</seg>
					<seg type="syl" met="-">di;</seg>
				</l>
				<l rhyme="a" met="---++--+-+-" n="endecasillabo">
					<!-- INPUT: Al-l’ar-mo-nia+ gli_a+ni-mi_al-trui+ pre-pa+ra =A -->
					<seg type="syl" met="-">Al</seg>
					<seg type="syl" met="-">l’ar</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">nia </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>a</seg>
					<seg type="syl" met="-">ni</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>al</seg>
					<seg type="syl" met="+">trui </seg>
					<seg type="syl" met="-">pre</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="b" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Con dol+ci ri-cer-ca+te_in bas+si mo+di: =B -->
					<seg type="syl" met="-">Con </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="-">cer</seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">te<c met="_"> </c>in </seg>
					<seg type="syl" met="+">bas</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">di:</seg>
				</l>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Co-sì+ co-stei,+ che nel+la do+glia_a-ma+ra =A -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">stei, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">nel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">glia<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="b" met="-+---++--+-" n="endecasillabo">
					<!-- INPUT: Già tut+te non ob-lia+ l’ar+ti_e le fro+di; =B -->
					<seg type="syl" met="-">Già </seg>
					<seg type="syl" met="+">tut</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">ob</seg>
					<seg type="syl" met="+">lia </seg>
					<seg type="syl" met="+">l’ar</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fro</seg>
					<seg type="syl" met="-">di;</seg>
				</l>
				<l rhyme="c" met="---++--+-+-" n="endecasillabo">
					<!-- INPUT: Fa di sos-pir+ bre+ve con-cen+to_in pri+ma, =C -->
					<seg type="syl" met="-">Fa </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">sos</seg>
					<seg type="syl" met="+">pir </seg>
					<seg type="syl" met="+">bre</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">to<c met="_"> </c>in </seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">ma,</seg>
				</l>
				<l rhyme="c" met="--++---+-+-" n="endecasillabo">
					<!-- INPUT: Per dis-por+ l’al+ma_in cui le vo+ci_im-pri+ma. =C -->
					<seg type="syl" met="-">Per </seg>
					<seg type="syl" met="-">dis</seg>
					<seg type="syl" met="+">por </seg>
					<seg type="syl" met="+">l’al</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>in </seg>
					<seg type="syl" met="-">cui </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>im</seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">ma.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
