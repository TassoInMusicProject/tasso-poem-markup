	<text>
		<body>
			<lg>
				<l rhyme="a" met="+----+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Du+ra con-clu-si-on,+ che tut+te_at-to+sca =A+ -->
					<seg type="syl" met="+">Du</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="-">clu</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">on, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">tut</seg>
					<seg type="syl" met="-">te<c met="_"> </c>at</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">sca</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Le dol-cez+ze pas-sa+te._Hor, con qual ar+te, =B+ -->
					<seg type="syl" met="-">Le </seg>
					<seg type="syl" met="-">dol</seg>
					<seg type="syl" met="+">cez</seg>
					<seg type="syl" met="-">ze </seg>
					<seg type="syl" met="-">pas</seg>
					<seg type="syl" met="+">sa</seg>
					<seg type="syl" met="-">te.<c met="_"> </c>Hor, </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">qual </seg>
					<seg type="syl" met="+">ar</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="c" met="-+---+-" n="settenario">
					<!-- INPUT: Cru-del,+ tu mi tor-men+ti? =c -->
					<seg type="syl" met="-">Cru</seg>
					<seg type="syl" met="+">del, </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">tor</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">ti?</seg>
				</l>
				<l rhyme="d" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Po+co dun+que ti pa+re =d+ -->
					<seg type="syl" met="+">Po</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="+">dun</seg>
					<seg type="syl" met="-">que </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="e" met="--++-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Che_in-fe-li+ce_i+o si+a, =e+ -->
					<seg type="syl" met="-">Che<c met="_"> </c>in</seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="+">ce<c met="_"> </c>i</seg>
					<seg type="syl" met="-">o </seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">a,</seg>
				</l>
				<l rhyme="e" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Ch'ac-cre+scer vie+ni la mi-se+ria mi+a? =E -->
					<seg type="syl" met="-">Ch'ac</seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">scer </seg>
					<seg type="syl" met="+">vie</seg>
					<seg type="syl" met="-">ni </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">ria </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">a?</seg>
				</l>
