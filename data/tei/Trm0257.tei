	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ar+di,_A-mor,+ se ti pia+ce, =a+ -->
					<seg type="syl" met="+">Ar</seg>
					<seg type="syl" met="-">di,<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ce,</seg>
				</l>
				<l rhyme="b" met="+----+-" n="settenario">
					<!-- INPUT: L'al+ma mia non che_'l fian+co, =b -->
					<seg type="syl" met="+">L'al</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">che<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">fian</seg>
					<seg type="syl" met="-">co,</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Ch'io non sa-rò+ di sof-fe-rir+ mai stan+co: =B -->
					<seg type="syl" met="-">Ch'io </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">sof</seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">mai </seg>
					<seg type="syl" met="+">stan</seg>
					<seg type="syl" met="-">co:</seg>
				</l>
				<l rhyme="a" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ma sem+bri la tua fa+ce =a+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">bri </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">tua </seg>
					<seg type="syl" met="+">fa</seg>
					<seg type="syl" met="-">ce</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario">
					<!-- INPUT: Ful-gor,+ ch'ad-den+tro pas+sa, =c -->
					<seg type="syl" met="-">Ful</seg>
					<seg type="syl" met="+">gor, </seg>
					<seg type="syl" met="-">ch'ad</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">tro </seg>
					<seg type="syl" met="+">pas</seg>
					<seg type="syl" met="-">sa,</seg>
				</l>
				<l rhyme="c" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: E fuor di sé+ ve-sti+gio_a pe+na las+sa. =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">fuor </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">sé </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">gio<c met="_"> </c>a </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">las</seg>
					<seg type="syl" met="-">sa.</seg>
				</l>
				<l rhyme="d" met="+----+-" n="settenario" enjamb="yes">
					<!-- INPUT: Por+ti-no_in me tuoi sde+gni =d+ -->
					<seg type="syl" met="+">Por</seg>
					<seg type="syl" met="-">ti</seg>
					<seg type="syl" met="-">no<c met="_"> </c>in </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">tuoi </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gni</seg>
				</l>
				<l rhyme="d" met="+--+-+-" n="settenario">
					<!-- INPUT: An+zi mar-tir+ che se+gni. =d -->
					<seg type="syl" met="+">An</seg>
					<seg type="syl" met="-">zi </seg>
					<seg type="syl" met="-">mar</seg>
					<seg type="syl" met="+">tir </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">gni.</seg>
				</l>
				<l rhyme="e" met="---+-+-" n="settenario">
					<!-- INPUT: Pur, se re-star+ ve-sti+gi =e -->
					<seg type="syl" met="-">Pur, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">re</seg>
					<seg type="syl" met="+">star </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">gi</seg>
				</l>
				<l rhyme="e" met="+----++--+-" n="endecasillabo">
					<!-- INPUT: Deb+bon di quel mar-ti+re_on+de m'af-flig+gi, =E -->
					<seg type="syl" met="+">Deb</seg>
					<seg type="syl" met="-">bon </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="-">mar</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="+">re<c met="_"> </c>on</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">m'af</seg>
					<seg type="syl" met="+">flig</seg>
					<seg type="syl" met="-">gi,</seg>
				</l>
				<l rhyme="f" met="-+--+--+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Di-mo+strin le lab+bra_e le mi+e go+te =F+ -->
					<seg type="syl" met="-">Di</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">strin </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">lab</seg>
					<seg type="syl" met="-">bra<c met="_"> </c>e </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">e </seg>
					<seg type="syl" met="+">go</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="f" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Di ca+ri ba+ci_im-pres+se_ar-den+ti no+te. =F -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">ba</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>im</seg>
					<seg type="syl" met="+">pres</seg>
					<seg type="syl" met="-">se<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
			</lg>
		</body>
	</text>
