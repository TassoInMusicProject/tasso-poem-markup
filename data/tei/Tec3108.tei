	<text>
		<body>
			<lg>
				<l rhyme="a" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: Non so-spi-rar,+ Pa-stor,+ non so-spi-ra+re; =A -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="-">spi</seg>
					<seg type="syl" met="+">rar, </seg>
					<seg type="syl" met="-">Pa</seg>
					<seg type="syl" met="+">stor, </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="-">spi</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">re;</seg>
				</l>
				<l rhyme="a" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Que+ste la+gri-me_a-ma+re =a+ -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">ste </seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">gri</seg>
					<seg type="syl" met="-">me<c met="_"> </c>a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Che spar+gi da' tuoi lu+mi, =b+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">spar</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">da' </seg>
					<seg type="syl" met="-">tuoi </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">mi,</seg>
				</l>
				<l rhyme="a" met="---+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Non spe-gne-ran+ scin-til+la de l'ar-do+re =A+ -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">spe</seg>
					<seg type="syl" met="-">gne</seg>
					<seg type="syl" met="+">ran </seg>
					<seg type="syl" met="-">scin</seg>
					<seg type="syl" met="+">til</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">l'ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario">
					<!-- INPUT: O+ve_o-gnor+ ti con-su+mi: =b -->
					<seg type="syl" met="+">O</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>o</seg>
					<seg type="syl" met="+">gnor </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">su</seg>
					<seg type="syl" met="-">mi:</seg>
				</l>
				<l rhyme="a" met="+-+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ché+ se_A-mor+ da le fiam+me de_'l tuo co+re =A+ -->
					<seg type="syl" met="+">Ché </seg>
					<seg type="syl" met="-">se<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fiam</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">de<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="c" met="++-+-+-" n="settenario">
					<!-- INPUT: Può+ trar+re_u-mo+re_e ven+ti, =c -->
					<seg type="syl" met="+">Può </seg>
					<seg type="syl" met="+">trar</seg>
					<seg type="syl" met="-">re<c met="_"> </c>u</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">ti,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Trar-rà+ da_'l pian+to_an-cor+ fa-vil+le_ar-den+ti. =C -->
					<seg type="syl" met="-">Trar</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="-">da<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">to<c met="_"> </c>an</seg>
					<seg type="syl" met="+">cor </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">vil</seg>
					<seg type="syl" met="-">le<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">ti.</seg>
				</l>
			</lg>
		</body>
