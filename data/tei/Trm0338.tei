	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Don+na cor-te+se_e bel+la, =a+ -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">cor</seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">se<c met="_"> </c>e </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la,</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario">
					<!-- INPUT: Deh! non vo-ler+ ch’io mo+ia =b -->
					<seg type="syl" met="-">Deh! </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">vo</seg>
					<seg type="syl" met="+">ler </seg>
					<seg type="syl" met="-">ch’io </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">ia</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: Di te-men+za_e di no+ia; =b -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">te</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">za<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ia;</seg>
				</l>
				<l rhyme="a" met="+--+--+--+-" n="endecasillabo">
					<!-- INPUT: Li+be-ra_il cor+po_e fa l’a+ni-ma_an-cel+la: =A -->
					<seg type="syl" met="+">Li</seg>
					<seg type="syl" met="-">be</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>il </seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">po<c met="_"> </c>e </seg>
					<seg type="syl" met="-">fa </seg>
					<seg type="syl" met="+">l’a</seg>
					<seg type="syl" met="-">ni</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>an</seg>
					<seg type="syl" met="+">cel</seg>
					<seg type="syl" met="-">la:</seg>
				</l>
				<l rhyme="c" met="---+---+++-" n="endecasillabo">
					<!-- INPUT: E se dis-de+gni si-gno-ria+ sì+ bas+sa, =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">dis</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">gni </seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">gno</seg>
					<seg type="syl" met="+">ria </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">bas</seg>
					<seg type="syl" met="-">sa,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario">
					<!-- INPUT: Al-trui+ mi do+na_e las+sa; =c -->
					<seg type="syl" met="-">Al</seg>
					<seg type="syl" met="+">trui </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">na<c met="_"> </c>e </seg>
					<seg type="syl" met="+">las</seg>
					<seg type="syl" met="-">sa;</seg>
				</l>
				<l rhyme="d" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: Ché+ tra’ pa-sto+ri for+se_o tra’ bi-fol+ci =D -->
					<seg type="syl" met="+">Ché </seg>
					<seg type="syl" met="-">tra’ </seg>
					<seg type="syl" met="-">pa</seg>
					<seg type="syl" met="+">sto</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se<c met="_"> </c>o </seg>
					<seg type="syl" met="-">tra’ </seg>
					<seg type="syl" met="-">bi</seg>
					<seg type="syl" met="+">fol</seg>
					<seg type="syl" met="-">ci</seg>
				</l>
				<l rhyme="d" met="-++-++-" n="settenario">
					<!-- INPUT: Av-rò+ l’o+re più+ dol+ci. =d -->
					<seg type="syl" met="-">Av</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="+">l’o</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ci.</seg>
				</l>
