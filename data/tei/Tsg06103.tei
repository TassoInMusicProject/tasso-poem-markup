<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E+ra la not+te,_e_’l suo stel-la+to ve+lo =A+ -->
					<seg type="syl" met="+">E</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">not</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>e<c met="_"> </c>’l </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="-">stel</seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">lo</seg>
				</l>
				<l rhyme="b" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Chia+ro spie-ga+va_e sen+za nu+be_al-cu+na; =B -->
					<seg type="syl" met="+">Chia</seg>
					<seg type="syl" met="-">ro </seg>
					<seg type="syl" met="-">spie</seg>
					<seg type="syl" met="+">ga</seg>
					<seg type="syl" met="-">va<c met="_"> </c>e </seg>
					<seg type="syl" met="+">sen</seg>
					<seg type="syl" met="-">za </seg>
					<seg type="syl" met="+">nu</seg>
					<seg type="syl" met="-">be<c met="_"> </c>al</seg>
					<seg type="syl" met="+">cu</seg>
					<seg type="syl" met="-">na;</seg>
				</l>
				<l rhyme="a" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E giá spar-gea+ rai lu-mi-no+si_e ge+lo =A+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">giá </seg>
					<seg type="syl" met="-">spar</seg>
					<seg type="syl" met="+">gea </seg>
					<seg type="syl" met="-">rai </seg>
					<seg type="syl" met="-">lu</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">si<c met="_"> </c>e </seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">lo</seg>
				</l>
				<l rhyme="b" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Di vi+ve per+le la sor-gen+te lu+na. =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="+">per</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">sor</seg>
					<seg type="syl" met="+">gen</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">na.</seg>
				</l>
				<l rhyme="a" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: L’in-na-mo-ra+ta don+na_i-va co_'l cie+lo =A -->
					<seg type="syl" met="-">L’in</seg>
					<seg type="syl" met="-">na</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="+">don</seg>
					<seg type="syl" met="-">na<c met="_"> </c>i</seg>
					<seg type="syl" met="-">va </seg>
					<seg type="syl" met="-">co<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Le sue fiam+me sfo-gan+do_ad u+na_ad u+na; =B -->
					<seg type="syl" met="-">Le </seg>
					<seg type="syl" met="-">sue </seg>
					<seg type="syl" met="+">fiam</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">sfo</seg>
					<seg type="syl" met="+">gan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>ad </seg>
					<seg type="syl" met="+">u</seg>
					<seg type="syl" met="-">na<c met="_"> </c>ad </seg>
					<seg type="syl" met="+">u</seg>
					<seg type="syl" met="-">na;</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E se-cre-ta+ri del suo_a-mo+re_an-ti+co =C+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="-">cre</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">suo<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re<c met="_"> </c>an</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">co</seg>
				</l>
				<l rhyme="c" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: Fea_i mu+ti cam+pi_e quel si-len+zio_a-mi+co. =C -->
					<seg type="syl" met="-">Fea<c met="_"> </c>i </seg>
					<seg type="syl" met="+">mu</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">cam</seg>
					<seg type="syl" met="-">pi<c met="_"> </c>e </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">len</seg>
					<seg type="syl" met="-">zio<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">co.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
