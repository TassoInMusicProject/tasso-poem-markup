	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Sov+ra le ver+di chio+me =a -->
					<seg type="syl" met="+">Sov</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">chio</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Di que+sto no+vo Lau+ro,_u-di+te co+me =A -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">vo </seg>
					<seg type="syl" met="+">Lau</seg>
					<seg type="syl" met="-">ro,<c met="_"> </c>u</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: De' ca-no+ri_au-gel-let+ti =b -->
					<seg type="syl" met="-">De' </seg>
					<seg type="syl" met="-">ca</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>au</seg>
					<seg type="syl" met="-">gel</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Al+tri scher-zan+do van di ra+mo_in ra+mo, =D -->
					<seg type="syl" met="+">Al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="-">scher</seg>
					<seg type="syl" met="+">zan</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">van </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>in </seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">mo,</seg>
				</l>
				<l rhyme="d" met="-+-+-+-" n="settenario">
					<!-- INPUT: Can-tan+do_&mdash;_io t'a+mo,_io t'a+mo_&mdash;; =d -->
					<seg type="syl" met="-">Can</seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>&amp;mdash;<c met="_"> </c>io </seg>
					<seg type="syl" met="+">t'a</seg>
					<seg type="syl" met="-">mo,<c met="_"> </c>io </seg>
					<seg type="syl" met="+">t'a</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>&amp;mdash;;</seg>
				</l>
				<l rhyme="e" met="-----+-" n="settenario">
					<!-- INPUT: Et ei par gli ri-spon+da =e -->
					<seg type="syl" met="-">Et </seg>
					<seg type="syl" met="-">ei </seg>
					<seg type="syl" met="-">par </seg>
					<seg type="syl" met="-">gli </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">spon</seg>
					<seg type="syl" met="-">da</seg>
				</l>
				<l rhyme="f" met="-+---+-" n="settenario">
					<!-- INPUT: Col dol+ce mor-mo-ri+o =f -->
					<seg type="syl" met="-">Col </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">mor</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="e" met="---+-+--" n="8">
					<!-- INPUT: De la tre-man+te fron+da &mdash; =e -->
					<seg type="syl" met="-">De </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">tre</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">fron</seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="f" met="++-+-+-" n="settenario">
					<!-- INPUT: Sì,+ sì,+ che v'a+mo_an-ch'i+o_&mdash;; =f -->
					<seg type="syl" met="+">Sì, </seg>
					<seg type="syl" met="+">sì, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">v'a</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>an</seg>
					<seg type="syl" met="+">ch'i</seg>
					<seg type="syl" met="-">o<c met="_"> </c>&amp;mdash;;</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ed al+tri vez-zo-set+ti =b+ -->
					<seg type="syl" met="-">Ed </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="-">vez</seg>
					<seg type="syl" met="-">zo</seg>
					<seg type="syl" met="+">set</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="g" met="+---+-+-" n="8">
					<!-- INPUT: Can+ta-no, &mdash; qui+vi, qui+vi_&mdash;, =g -->
					<seg type="syl" met="+">Can</seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="-">no, </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="+">qui</seg>
					<seg type="syl" met="-">vi, </seg>
					<seg type="syl" met="+">qui</seg>
					<seg type="syl" met="-">vi<c met="_"> </c>&amp;mdash;,</seg>
				</l>
				<l rhyme="g" met="+-+-+---+-+-" n="12">
					<!-- INPUT: Qua+si vo+glian di+re &mdash; In que+sti ri+vi, =G -->
					<seg type="syl" met="+">Qua</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">glian </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sti </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">vi,</seg>
				</l>
				<l rhyme="h" met="-+-+-+-" n="settenario">
					<!-- INPUT: O_in-tor+no_a que+ste lin+fe =h -->
					<seg type="syl" met="-">O<c met="_"> </c>in</seg>
					<seg type="syl" met="+">tor</seg>
					<seg type="syl" met="-">no<c met="_"> </c>a </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">ste </seg>
					<seg type="syl" met="+">lin</seg>
					<seg type="syl" met="-">fe</seg>
				</l>
				<l rhyme="h" met="---+-+--" n="8">
					<!-- INPUT: Ti va-gheg-gian+ le Nin+fe. &mdash; =h -->
					<seg type="syl" met="-">Ti </seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="-">gheg</seg>
					<seg type="syl" met="+">gian </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">Nin</seg>
					<seg type="syl" met="-">fe. </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
			</lg>
		</body>
