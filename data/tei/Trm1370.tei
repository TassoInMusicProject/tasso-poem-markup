	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Qui do+ve fan le pian+te =a+ -->
					<seg type="syl" met="-">Qui </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">fan </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="+--+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ver+di,_e fron-do+se,_e fo+sche,_e l'er+be_e_i fio+ri =B+ -->
					<seg type="syl" met="+">Ver</seg>
					<seg type="syl" met="-">di,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">fron</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">se,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="-">sche,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">l'er</seg>
					<seg type="syl" met="-">be<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Seg+gio_e di-fe+sa da gl'e-sti+vi_ar-do+ri, =B -->
					<seg type="syl" met="+">Seg</seg>
					<seg type="syl" met="-">gio<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">gl'e</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">vi<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ri-tro+va_il gran+de_HEN-RI+CO =c+ -->
					<seg type="syl" met="-">Ri</seg>
					<seg type="syl" met="+">tro</seg>
					<seg type="syl" met="-">va<c met="_"> </c>il </seg>
					<seg type="syl" met="+">gran</seg>
					<seg type="syl" met="-">de<c met="_"> </c>HEN</seg>
					<seg type="syl" met="+">RI</seg>
					<seg type="syl" met="-">CO</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario">
					<!-- INPUT: L'om+bre, l'au+re_e gl'o-do+ri =b -->
					<seg type="syl" met="+">L'om</seg>
					<seg type="syl" met="-">bre, </seg>
					<seg type="syl" met="+">l'au</seg>
					<seg type="syl" met="-">re<c met="_"> </c>e </seg>
					<seg type="syl" met="-">gl'o</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="c" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: E sog-get+ta la ter+ra_e_'l cie+lo_a-mi+co. =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">sog</seg>
					<seg type="syl" met="+">get</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>e<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">co.</seg>
				</l>
				<l rhyme="d" met="++-+++-" n="settenario" enjamb="yes">
					<!-- INPUT: Né+ già+ ne-gar+ dol+ci_ac+que =d+ -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="+">già </seg>
					<seg type="syl" met="-">ne</seg>
					<seg type="syl" met="+">gar </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="+">ci<c met="_"> </c>ac</seg>
					<seg type="syl" met="-">que</seg>
				</l>
				<l rhyme="e" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: I va+ghi ri+vi_e_i fon+ti =e+ -->
					<seg type="syl" met="-">I </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ghi </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">vi<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">fon</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="e" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: E per se-cre+te vie gl'al-pe+stri mon+ti, =E -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">vie </seg>
					<seg type="syl" met="-">gl'al</seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">stri </seg>
					<seg type="syl" met="+">mon</seg>
					<seg type="syl" met="-">ti,</seg>
				</l>
				<l rhyme="d" met="--+----+-+-" n="endecasillabo">
					<!-- INPUT: Ma rac-cor+le dal ciel via più+ gli piac+que; =D -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">rac</seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">dal </seg>
					<seg type="syl" met="-">ciel </seg>
					<seg type="syl" met="-">via </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">gli </seg>
					<seg type="syl" met="+">piac</seg>
					<seg type="syl" met="-">que;</seg>
				</l>
				<l rhyme="f" met="--+--+-" n="settenario">
					<!-- INPUT: Voi, che pri+ma tor-re+ste? =f -->
					<seg type="syl" met="-">Voi, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">pri</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">tor</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">ste?</seg>
				</l>
				<l rhyme="f" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: Tri-bu+to da la ter+ra_o don ce-le+ste? =F -->
					<seg type="syl" met="-">Tri</seg>
					<seg type="syl" met="+">bu</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>o </seg>
					<seg type="syl" met="-">don </seg>
					<seg type="syl" met="-">ce</seg>
					<seg type="syl" met="+">le</seg>
					<seg type="syl" met="-">ste?</seg>
				</l>
			</lg>
		</body>
	</text>
