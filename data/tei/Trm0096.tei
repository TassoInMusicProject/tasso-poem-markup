<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Don+na, quan+to più_a den+tro =a -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na, </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">più<c met="_"> </c>a </seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">tro</seg>
				</l>
				<l rhyme="b" met="-+-+-+-" n="settenario">
					<!-- INPUT: Co-nob+bi_il vo+stro co+re, =b -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">nob</seg>
					<seg type="syl" met="-">bi<c met="_"> </c>il </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: Tan+to_a dar+vi cre-den+za_io son più tar+do, =C -->
					<seg type="syl" met="+">Tan</seg>
					<seg type="syl" met="-">to<c met="_"> </c>a </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">cre</seg>
					<seg type="syl" met="+">den</seg>
					<seg type="syl" met="-">za<c met="_"> </c>io </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="+">tar</seg>
					<seg type="syl" met="-">do,</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario">
					<!-- INPUT: Né sti+mo quel di fò+re, =b -->
					<seg type="syl" met="-">Né </seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">mo </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">fò</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Io di+co_un va+go_in-chi+no,_un dol+ce sguar+do, =C -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">co<c met="_"> </c>un </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">go<c met="_"> </c>in</seg>
					<seg type="syl" met="+">chi</seg>
					<seg type="syl" met="-">no,<c met="_"> </c>un </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">sguar</seg>
					<seg type="syl" met="-">do,</seg>
				</l>
				<l rhyme="c" met="----++--" n="8">
					<!-- INPUT: Un dir &mdash; nel fo+co_io_ar+do &mdash;, =c -->
					<seg type="syl" met="-">Un </seg>
					<seg type="syl" met="-">dir </seg>
					<seg type="syl" met="-">&amp;mdash; </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="+">co<c met="_"> </c>io<c met="_"> </c>ar</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">&amp;mdash;,</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario">
					<!-- INPUT: Un sco-lo-rir+ di vi+so, =d -->
					<seg type="syl" met="-">Un </seg>
					<seg type="syl" met="-">sco</seg>
					<seg type="syl" met="-">lo</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">so,</seg>
				</l>
				<l rhyme="d" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Un do-len+te so-spi+ro,_un lie+to ri+so. =D -->
					<seg type="syl" met="-">Un </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">len</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">spi</seg>
					<seg type="syl" met="-">ro,<c met="_"> </c>un </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">so.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
