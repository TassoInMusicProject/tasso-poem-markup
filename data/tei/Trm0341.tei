	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Fug+gi, fug+gi, do-lor,+ da que+sto pet+to =A -->
					<seg type="syl" met="+">Fug</seg>
					<seg type="syl" met="-">gi, </seg>
					<seg type="syl" met="+">fug</seg>
					<seg type="syl" met="-">gi, </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">lor, </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Or che vi tor+na la gio-io+sa spe+me; =B -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">tor</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="+">io</seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">me;</seg>
				</l>
				<l rhyme="a" met="---+--+--+-" n="endecasillabo">
					<!-- INPUT: Or che pro-met+te_al cor pa+ce_e di-let+to, =A -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">pro</seg>
					<seg type="syl" met="+">met</seg>
					<seg type="syl" met="-">te<c met="_"> </c>al </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="c" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Tut+ti fug-gi+te_o-mai,+ tor-men+ti_e pe+ne. =C -->
					<seg type="syl" met="+">Tut</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">fug</seg>
					<seg type="syl" met="+">gi</seg>
					<seg type="syl" met="-">te<c met="_"> </c>o</seg>
					<seg type="syl" met="+">mai, </seg>
					<seg type="syl" met="-">tor</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">ne.</seg>
				</l>
				<l rhyme="d" met="+-++-+-+-+-" n="endecasillabo">
					<!-- INPUT: Già+ vi-ci+no_è_'l+ mio so+le:_oh cie+li_a-mi+ci! =D -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="+">no<c met="_"> </c>è<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">le:<c met="_"> </c>oh </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">li<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">ci!</seg>
				</l>
				<l rhyme="d" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Già+ s'ap-pres+sa_il mio be+ne:_oh dì+ fe-li+ci! =D -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">s'ap</seg>
					<seg type="syl" met="+">pres</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="-">ne:<c met="_"> </c>oh </seg>
					<seg type="syl" met="+">dì </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ci!</seg>
				</l>
				<l rhyme="e" met="+-+--++--+-" n="endecasillabo">
					<!-- INPUT: Né,+ po-ten+do tor-nar+ sen+za par-ti+ta, =E -->
					<seg type="syl" met="+">Né, </seg>
					<seg type="syl" met="-">po</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">tor</seg>
					<seg type="syl" met="+">nar </seg>
					<seg type="syl" met="+">sen</seg>
					<seg type="syl" met="-">za </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="e" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Mi pia+ce che par-tì+ la ca+ra vi+ta. =E -->
					<seg type="syl" met="-">Mi </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="+">tì </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta.</seg>
				</l>
			</lg>
		</body>
	</text>
