	<text>
		<body>
			<lg>
				<l rhyme="a" met="--++--+-" n="8">
					<!-- INPUT: AMINTA: Hor-sù,+ sco+pri-mi_il tut+to. =a -->
					<seg type="syl" met="-">AMINTA: </seg>
					<seg type="syl" met="-">Hor</seg>
					<seg type="syl" met="+">sù, </seg>
					<seg type="syl" met="+">sco</seg>
					<seg type="syl" met="-">pri</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>il </seg>
					<seg type="syl" met="+">tut</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="b" met="-+-" n="3">
					<!-- INPUT: TIRSI: O+di. =b -->
					<seg type="syl" met="-">TIRSI: </seg>
					<seg type="syl" met="+">O</seg>
					<seg type="syl" met="-">di.</seg>
				</l>
				<l rhyme="c" met="--+-" n="4">
					<!-- INPUT: AMINTA: Di' to+sto. =c -->
					<seg type="syl" met="-">AMINTA: </seg>
					<seg type="syl" met="-">Di' </seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">sto.</seg>
				</l>
				<l rhyme="d" met="-+-++-+-+-+-" n="12">
					<!-- INPUT: TIRSI: Sil+via_at-ten+de_a_u+na fon+te,_i-gnu+da_e so+la. =D -->
					<seg type="syl" met="-">TIRSI: </seg>
					<seg type="syl" met="+">Sil</seg>
					<seg type="syl" met="-">via<c met="_"> </c>at</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="+">de<c met="_"> </c>a<c met="_"> </c>u</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">fon</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>i</seg>
					<seg type="syl" met="+">gnu</seg>
					<seg type="syl" met="-">da<c met="_"> </c>e </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">la.</seg>
				</l>
				<l rhyme="e" met="--+--+-" n="settenario">
					<!-- INPUT: Ar-di-rai+ tu d'an-dar+vi? =e -->
					<seg type="syl" met="-">Ar</seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">rai </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">d'an</seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">vi?</seg>
				</l>
				<l rhyme="f" met="----+-" n="6">
					<!-- INPUT: AMINTA: O, che mi di+ci? =f -->
					<seg type="syl" met="-">AMINTA: </seg>
					<seg type="syl" met="-">O, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">ci?</seg>
				</l>
				<l rhyme="d" met="---+-+-+-" n="9">
					<!-- INPUT: Sil-via m'at-ten+de_i-gnu+da_e so+la? =d -->
					<seg type="syl" met="-">Sil</seg>
					<seg type="syl" met="-">via </seg>
					<seg type="syl" met="-">m'at</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">de<c met="_"> </c>i</seg>
					<seg type="syl" met="+">gnu</seg>
					<seg type="syl" met="-">da<c met="_"> </c>e </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">la?</seg>
				</l>
				<l rhyme="d" met="-+-" n="3">
					<!-- INPUT: TIRSI: So+la, =d -->
					<seg type="syl" met="-">TIRSI: </seg>
					<seg type="syl" met="+">So</seg>
					<seg type="syl" met="-">la,</seg>
				</l>
				<l rhyme="g" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Se non quan+to v'è Daf+ne, ch'è+ per no+i. =G -->
					<seg type="syl" met="-">Se </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">v'è </seg>
					<seg type="syl" met="+">Daf</seg>
					<seg type="syl" met="-">ne, </seg>
					<seg type="syl" met="+">ch'è </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">i.</seg>
				</l>
				<l rhyme="h" met="--++--+-" n="8">
					<!-- INPUT: AMINTA: I-gnu+da_el+la m'a-spet+ta? =h -->
					<seg type="syl" met="-">AMINTA: </seg>
					<seg type="syl" met="-">I</seg>
					<seg type="syl" met="+">gnu</seg>
					<seg type="syl" met="+">da<c met="_"> </c>el</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">m'a</seg>
					<seg type="syl" met="+">spet</seg>
					<seg type="syl" met="-">ta?</seg>
				</l>
				<l rhyme="i" met="--+--" n="5">
					<!-- INPUT: TIRSI: I-gnu+da, ma... =i -->
					<seg type="syl" met="-">TIRSI: </seg>
					<seg type="syl" met="-">I</seg>
					<seg type="syl" met="+">gnu</seg>
					<seg type="syl" met="-">da, </seg>
					<seg type="syl" met="-">ma...</seg>
				</l>
				<l rhyme="b" met="--+---+---+-" n="12">
					<!-- INPUT: AMINTA: Ohi-mè,+ che ma? Tu ta+ci; tu m'uc-ci+di. =B -->
					<seg type="syl" met="-">AMINTA: </seg>
					<seg type="syl" met="-">Ohi</seg>
					<seg type="syl" met="+">mè, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">ma? </seg>
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">ci; </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">m'uc</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">di.</seg>
				</l>
				<l rhyme="j" met="----+--+--+-" n="12">
					<!-- INPUT: TIRSI: Ma non sa già+ che tu v'hab+bi d'an-da+re. =J -->
					<seg type="syl" met="-">TIRSI: </seg>
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">sa </seg>
					<seg type="syl" met="+">già </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="+">v'hab</seg>
					<seg type="syl" met="-">bi </seg>
					<seg type="syl" met="-">d'an</seg>
					<seg type="syl" met="+">da</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
