					<seg type="syl" met="+">dor</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="a" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Pian+gi do-len+te_or con Ma-ria+ che pian+ge =A -->
					<seg type="syl" met="+">Pian</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">len</seg>
					<seg type="syl" met="-">te<c met="_"> </c>or </seg>
