	<text>
		<body>
			<lg>
				<l rhyme="a" met="+----++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Chia+ro sol, che ro-tan+do_e+sci del Gan+ge, =A+ -->
