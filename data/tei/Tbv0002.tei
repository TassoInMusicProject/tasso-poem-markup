					<seg type="syl" met="-">pio</seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">so </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">por</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>e<c met="_"> </c>o</seg>
					<seg type="syl" met="+">scu</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>il </seg>
					<seg type="syl" met="+">gior</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="a" met="-+-----+-+-" n="endecasillabo" enjamb="yes">
