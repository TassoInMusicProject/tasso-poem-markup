				</l>
				<l rhyme="c" met="---+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: E voi spar-ge+te_an-cor+ di pian+to_un nem+bo, =C+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">spar</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">te<c met="_"> </c>an</seg>
