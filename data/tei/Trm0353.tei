	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+-+-+-" n="8">
					<!-- INPUT: Già+ fui ca+ro_a gli_oc+chi tuo+i, =a -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">fui </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>a </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="+">tuo</seg>
					<seg type="syl" met="-">i,</seg>
				</l>
				<l rhyme="b" met="--+-" n="4">
					<!-- INPUT: O mio so+le; =b -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">le;</seg>
				</l>
				<l rhyme="c" met="+-+---+-" n="8">
					<!-- INPUT: Vis+si lie+to del tuo_a-mo+re, =c -->
					<seg type="syl" met="+">Vis</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">tuo<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="--+-" n="4">
					<!-- INPUT: Del mio_ar-do+re; =c -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="-">mio<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">re;</seg>
				</l>
				<l rhyme="d" met="--+-+-" n="6">
					<!-- INPUT: Or che più+ non m'a+mi =d -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">m'a</seg>
					<seg type="syl" met="-">mi</seg>
				</l>
				<l rhyme="d" met="-+-+--++-+-" n="endecasillabo">
					<!-- INPUT: Co-m'es+ser può+ che mai più+ vi+ver bra+mi? =D -->
					<seg type="syl" met="-">Co</seg>
					<seg type="syl" met="+">m'es</seg>
					<seg type="syl" met="-">ser </seg>
					<seg type="syl" met="+">può </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">mai </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ver </seg>
					<seg type="syl" met="+">bra</seg>
					<seg type="syl" met="-">mi?</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="e" met="--+-+-+-" n="8">
					<!-- INPUT: Tu pur ve+di_il pian+to mi+o, =e -->
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">di<c met="_"> </c>il </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">o,</seg>
				</l>
				<l rhyme="f" met="--+-" n="4">
					<!-- INPUT: O mia gio+ia, =f -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">gio</seg>
					<seg type="syl" met="-">ia,</seg>
				</l>
				<l rhyme="g" met="--+---+-" n="8" enjamb="yes">
					<!-- INPUT: Sai ch'io t'a+mo, ch'io t'a-do+ro, =g+ -->
					<seg type="syl" met="-">Sai </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="+">t'a</seg>
					<seg type="syl" met="-">mo, </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="-">t'a</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ro,</seg>
				</l>
				<l rhyme="g" met="--+-" n="4">
					<!-- INPUT: Ch'io mi mo+ro, =g -->
					<seg type="syl" met="-">Ch'io </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">ro,</seg>
				</l>
				<l rhyme="b" met="----+-" n="6">
					<!-- INPUT: E mi sei cru-de+le! =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">sei </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">le!</seg>
				</l>
				<l rhyme="b" met="++-+--+--+-" n="endecasillabo">
					<!-- INPUT: Que+sta_è+ mer-cé+ de l'a-mor+ mio fe-de+le? =b -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="+">sta<c met="_"> </c>è </seg>
					<seg type="syl" met="-">mer</seg>
					<seg type="syl" met="+">cé </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">l'a</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">le?</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="b" met="--+---+-" n="8">
					<!-- INPUT: Ma se go+di del mio ma+le, =b -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="+">go</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">le,</seg>
				</l>
				<l rhyme="h" met="--+-" n="4">
					<!-- INPUT: O mio be+ne, =h -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="-">ne,</seg>
				</l>
				<l rhyme="c" met="--+---+-" n="8" enjamb="yes">
					<!-- INPUT: Son con-ten+to di lan-gui+re, =c+ -->
					<seg type="syl" met="-">Son </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">ten</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">lan</seg>
					<seg type="syl" met="+">gui</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="c" met="--+-" n="4">
					<!-- INPUT: Di mo-ri+re, =c -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="a" met="--+-+-" n="6" enjamb="yes">
					<!-- INPUT: S'io ve-drò+ che po+i =a+ -->
					<seg type="syl" met="-">S'io </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">drò </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">po</seg>
					<seg type="syl" met="-">i</seg>
				</l>
				<l rhyme="a" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Il mio mo-rir+ sia ca+ro_a gli_oc+chi tuo+i. =A -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">sia </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>a </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="+">tuo</seg>
					<seg type="syl" met="-">i.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
