	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario">
					<!-- INPUT: Tu na-sce+sti di fur+to, =a -->
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="-">na</seg>
					<seg type="syl" met="+">sce</seg>
					<seg type="syl" met="-">sti </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">fur</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: Pic-cio-let+to Gre-chi+no, =b -->
					<seg type="syl" met="-">Pic</seg>
					<seg type="syl" met="-">cio</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">Gre</seg>
					<seg type="syl" met="+">chi</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Ma fu cer+to fe-li+ce_il tuo de-sti+no. =B -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="+">cer</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>il </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
				<l rhyme="c" met="+-+--+-" n="settenario">
					<!-- INPUT: Ché+ di fur+to non vi+vi, =c -->
					<seg type="syl" met="+">Ché </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">fur</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">vi,</seg>
				</l>
				<l rhyme="d" met="+--+-+-" n="settenario">
					<!-- INPUT: Né+ di ra-pi+na_an-co+ra; =d -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">na<c met="_"> </c>an</seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">ra;</seg>
				</l>
				<l rhyme="d" met="---+-----+-" n="endecasillabo">
					<!-- INPUT: E, se nu-tri+to pur ne sei ta-lo+ra, =D -->
					<seg type="syl" met="-">E, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">nu</seg>
					<seg type="syl" met="+">tri</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">sei </seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ra,</seg>
				</l>
				<l rhyme="e" met="-++----+++-" n="endecasillabo">
					<!-- INPUT: Il ci+bo_è+ tuo, ma la fa-ti+ca_è+ mi+a. =E -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="+">bo<c met="_"> </c>è </seg>
					<seg type="syl" met="-">tuo, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">fa</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="+">ca<c met="_"> </c>è </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">a.</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: E tu la fug+gi_e schi+vi =c -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fug</seg>
					<seg type="syl" met="-">gi<c met="_"> </c>e </seg>
					<seg type="syl" met="+">schi</seg>
					<seg type="syl" met="-">vi</seg>
				</l>
				<l rhyme="e" met="-+-++--+-+-" n="endecasillabo">
					<!-- INPUT: Nel dol+ce_al-ber+go_o+ve gio-ir+ so-li+a. =E -->
					<seg type="syl" met="-">Nel </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce<c met="_"> </c>al</seg>
					<seg type="syl" met="+">ber</seg>
					<seg type="syl" met="+">go<c met="_"> </c>o</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="+">ir </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">a.</seg>
				</l>
				<l rhyme="a" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: Las+so! ben-ché+ di fur+to_io non sia na+to, =A -->
					<seg type="syl" met="+">Las</seg>
					<seg type="syl" met="-">so! </seg>
					<seg type="syl" met="-">ben</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">fur</seg>
					<seg type="syl" met="-">to<c met="_"> </c>io </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">sia </seg>
					<seg type="syl" met="+">na</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="a" met="---+-+-" n="settenario">
					<!-- INPUT: Ho men be-ni+gno fa+to. =a -->
					<seg type="syl" met="-">Ho </seg>
					<seg type="syl" met="-">men </seg>
					<seg type="syl" met="-">be</seg>
					<seg type="syl" met="+">ni</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="+">fa</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
			</lg>
		</body>
	</text>
