	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: A+ni-me san+te_e bel+le, =a+ -->
					<seg type="syl" met="+">A</seg>
					<seg type="syl" met="-">ni</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">san</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">le,</seg>
				</l>
				<l rhyme="b" met="---+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Che da gli_af-fan+ni_u-ma+ni_e da' mar-ti+ri =B+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">gli<c met="_"> </c>af</seg>
					<seg type="syl" met="+">fan</seg>
					<seg type="syl" met="-">ni<c met="_"> </c>u</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ni<c met="_"> </c>e </seg>
					<seg type="syl" met="-">da' </seg>
					<seg type="syl" met="-">mar</seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Vo-la+ste_a' più+ sub-li+mi_ed al+ti gi+ri, =B+ -->
					<seg type="syl" met="-">Vo</seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">ste<c met="_"> </c>a' </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">sub</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">gi</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario">
					<!-- INPUT: Gra-di+te_i no+stri pre+ghi =c -->
					<seg type="syl" met="-">Gra</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">te<c met="_"> </c>i </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stri </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">ghi</seg>
				</l>
				<l rhyme="d" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: Su l'a+le de la fe+de_e de la spe+me; =D -->
					<seg type="syl" met="-">Su </seg>
					<seg type="syl" met="+">l'a</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">de<c met="_"> </c>e </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">me;</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario">
					<!-- INPUT: E pur co' vo+stri_in-sie+me =d -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">co' </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stri<c met="_"> </c>in</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="e" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: La giu-sti+zia su-per+na =e+ -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">giu</seg>
					<seg type="syl" met="+">sti</seg>
					<seg type="syl" met="-">zia </seg>
					<seg type="syl" met="-">su</seg>
					<seg type="syl" met="+">per</seg>
					<seg type="syl" met="-">na</seg>
				</l>
				<l rhyme="c" met="-+---+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: O-mai+ fuor di suo cor+so_a noi si spie+ghi, =C+ -->
					<seg type="syl" met="-">O</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">fuor </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">so<c met="_"> </c>a </seg>
					<seg type="syl" met="-">noi </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">spie</seg>
					<seg type="syl" met="-">ghi,</seg>
				</l>
				<l rhyme="f" met="--+--+-" n="settenario">
					<!-- INPUT: Pec-ca-to+ri,_e no_'l ce+lo, =f -->
					<seg type="syl" met="-">Pec</seg>
					<seg type="syl" met="-">ca</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">ri,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">no<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">lo,</seg>
				</l>
				<l rhyme="f" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Voi co-lon+ne del cie+lo, =f+ -->
					<seg type="syl" met="-">Voi </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">lon</seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo,</seg>
				</l>
				<l rhyme="g" met="+-+--+-" n="settenario">
					<!-- INPUT: An+zi_i cie+li voi se+te, =g -->
					<seg type="syl" met="+">An</seg>
					<seg type="syl" met="-">zi<c met="_"> </c>i </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">li </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="g" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Al+me fe-li+ci_e lie+te, =g+ -->
					<seg type="syl" met="+">Al</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>e </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="e" met="--+----+-+-" n="endecasillabo">
					<!-- INPUT: Che nar-ra+te di Dio la glo+ria_e-ter+na. =E -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">nar</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">Dio </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">glo</seg>
					<seg type="syl" met="-">ria<c met="_"> </c>e</seg>
					<seg type="syl" met="+">ter</seg>
					<seg type="syl" met="-">na.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
