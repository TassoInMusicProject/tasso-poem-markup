	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Tu, per+fi-do si-gnor,+ tu di-sle-a+le, =A+ -->
					<seg type="syl" met="-">Tu, </seg>
					<seg type="syl" met="+">per</seg>
					<seg type="syl" met="-">fi</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">gnor, </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="-">sle</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">le,</seg>
				</l>
				<l rhyme="b" met="-++---+--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Che sot+to_om+bra di ben co+pri_il mal ve+ro, =B+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">sot</seg>
					<seg type="syl" met="+">to<c met="_"> </c>om</seg>
					<seg type="syl" met="-">bra </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">ben </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">pri<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mal </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">ro,</seg>
				</l>
				<l rhyme="a" met="-+-+-+---+-" n="endecasillabo">
					<!-- INPUT: Og-get+to de+sti_im-pa+re_e di-se-gua+le, =A -->
					<seg type="syl" met="-">Og</seg>
					<seg type="syl" met="+">get</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">sti<c met="_"> </c>im</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">re<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">gua</seg>
					<seg type="syl" met="-">le,</seg>
				</l>
				<l rhyme="b" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: On+de_a pie+no m'af-fli+ga,_al mio pen-sie+ro. =B -->
					<seg type="syl" met="+">On</seg>
					<seg type="syl" met="-">de<c met="_"> </c>a </seg>
					<seg type="syl" met="+">pie</seg>
					<seg type="syl" met="-">no </seg>
					<seg type="syl" met="-">m'af</seg>
					<seg type="syl" met="+">fli</seg>
					<seg type="syl" met="-">ga,<c met="_"> </c>al </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">ro.</seg>
				</l>
				<l rhyme="a" met="+-+--+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Deh!+ mie stel+le cru-de+li,_or quan+do ta+le =A+ -->
					<seg type="syl" met="+">Deh! </seg>
					<seg type="syl" met="-">mie </seg>
					<seg type="syl" met="+">stel</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">li,<c met="_"> </c>or </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">do </seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="b" met="+--+--++-+-" n="endecasillabo">
					<!-- INPUT: Scem+pio fu vi+sto_e co-sì+ stra+no_e fe+ro? =B -->
					<seg type="syl" met="+">Scem</seg>
					<seg type="syl" met="-">pio </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">sto<c met="_"> </c>e </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">no<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">ro?</seg>
				</l>
				<l rhyme="c" met="++-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Ché+ do+ve_in al+tri_a-mor+ da spe+me na+sce, =C -->
					<seg type="syl" met="+">Ché </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>in </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">tri<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">spe</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">na</seg>
					<seg type="syl" met="-">sce,</seg>
				</l>
				<l rhyme="c" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Dal non spe-rar+ in me s'a-vi+va_e pa+sce. =C -->
					<seg type="syl" met="-">Dal </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">spe</seg>
					<seg type="syl" met="+">rar </seg>
					<seg type="syl" met="-">in </seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">s'a</seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">va<c met="_"> </c>e </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">sce.</seg>
				</l>
			</lg>
