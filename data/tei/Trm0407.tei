					<seg type="syl" met="-">ro<c met="_"> </c>e</seg>
					<seg type="syl" met="+">gli<c met="_"> </c>è </seg>
					<seg type="syl" met="-">d'A</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re?</seg>
				</l>
				<l rhyme="c" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Deh!+ chi tan+to vi-ci+ne =c+ -->
					<seg type="syl" met="+">Deh! </seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">vi</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">ne</seg>
				</l>
				<l rhyme="a" met="-+-+-+---+-" n="endecasillabo">
					<!-- INPUT: Le pe+ne_e_i dol+ci pre+mi_in lui ri-po+se? =A -->
					<seg type="syl" met="-">Le </seg>
					<seg type="syl" met="+">pe</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>in </seg>
					<seg type="syl" met="-">lui </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">po</seg>
					<seg type="syl" met="-">se?</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: E chi d'a-cu+te spi+ne =c+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">d'a</seg>
