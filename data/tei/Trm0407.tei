	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: La na-tu+ra com-po+se =a+ -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="-">na</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">com</seg>
					<seg type="syl" met="+">po</seg>
					<seg type="syl" met="-">se</seg>
				</l>
				<l rhyme="b" met="+--+-+-" n="settenario">
					<!-- INPUT: Que+st'o-do-ra+to fio+re, =b -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">st'o</seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="b" met="-----+-+-+-" n="endecasillabo">
					<!-- INPUT: O pur del ma-gi-ste+ro_e-gli_è+ d'A-mo+re? =B -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">ma</seg>
					<seg type="syl" met="-">gi</seg>
