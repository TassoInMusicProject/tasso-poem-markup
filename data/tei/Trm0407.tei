				</l>
				<l rhyme="d" met="+--+-+-" n="settenario">
					<!-- INPUT: Cin+se le bel+le fo+glie, =d -->
					<seg type="syl" met="+">Cin</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="-">glie,</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: On+de, s'in-cau+ta man ta-lo+ra_il co+glie, =D+ -->
					<seg type="syl" met="+">On</seg>
					<seg type="syl" met="-">de, </seg>
					<seg type="syl" met="-">s'in</seg>
					<seg type="syl" met="+">cau</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">man </seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>il </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">glie,</seg>
				</l>
				<l rhyme="e" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Pun+ta,_in un pun+to so+lo =e+ -->
					<seg type="syl" met="+">Pun</seg>
					<seg type="syl" met="-">ta,<c met="_"> </c>in </seg>
					<seg type="syl" met="-">un </seg>
					<seg type="syl" met="+">pun</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">lo</seg>
				</l>
				<l rhyme="e" met="+--+-+-" n="settenario">
					<!-- INPUT: Sen+te_al-le-grez+za_e duo+lo? =e -->
					<seg type="syl" met="+">Sen</seg>
					<seg type="syl" met="-">te<c met="_"> </c>al</seg>
					<seg type="syl" met="-">le</seg>
					<seg type="syl" met="+">grez</seg>
					<seg type="syl" met="-">za<c met="_"> </c>e </seg>
					<seg type="syl" met="+">duo</seg>
					<seg type="syl" met="-">lo?</seg>
				</l>
				<l rhyme="f" met="----+-+-++-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Oh fior me-ra-vi+glio-so,_on+d'an-cor+ du+ra =F+ -->
					<seg type="syl" met="-">Oh </seg>
					<seg type="syl" met="-">fior </seg>
					<seg type="syl" met="-">me</seg>
					<seg type="syl" met="-">ra</seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">glio</seg>
					<seg type="syl" met="+">so,<c met="_"> </c>on</seg>
					<seg type="syl" met="-">d'an</seg>
					<seg type="syl" met="+">cor </seg>
					<seg type="syl" met="+">du</seg>
					<seg type="syl" met="-">ra</seg>
				</l>
				<l rhyme="f" met="+-+--+---+-" n="endecasillabo">
					<!-- INPUT: Li+te_in-cer+ta d'A-mo+re_e di Na-tu+ra! =F -->
					<seg type="syl" met="+">Li</seg>
					<seg type="syl" met="-">te<c met="_"> </c>in</seg>
					<seg type="syl" met="+">cer</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">d'A</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re<c met="_"> </c>e </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">Na</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">ra!</seg>
				</l>
