	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Que+sta vo+stra pie-ta+te =a+ -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stra </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario">
					<!-- INPUT: Non re-fri-ge+rio_al co+re =b -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">re</seg>
					<seg type="syl" met="-">fri</seg>
					<seg type="syl" met="+">ge</seg>
					<seg type="syl" met="-">rio<c met="_"> </c>al </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="b" met="-++--+-" n="settenario">
					<!-- INPUT: Ma dà+ for+za_a l'ar-do+re; =b -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">dà </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">za<c met="_"> </c>a </seg>
					<seg type="syl" met="-">l'ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">re;</seg>
				</l>
				<l rhyme="a" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Dun+que d'es+ser pie-to+sa_o-mai+ ces-sa+te =A -->
					<seg type="syl" met="+">Dun</seg>
					<seg type="syl" met="-">que </seg>
					<seg type="syl" met="+">d'es</seg>
					<seg type="syl" met="-">ser </seg>
					<seg type="syl" met="-">pie</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>o</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">ces</seg>
					<seg type="syl" met="+">sa</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="c" met="--++-+-" n="settenario">
					<!-- INPUT: In co-sì+ stra+na gui+sa =c -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">gui</seg>
					<seg type="syl" met="-">sa</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: Che ne sia l'al+ma_uc-ci+sa; =c -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="-">sia </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>uc</seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">sa;</seg>
				</l>
				<l rhyme="d" met="-+---+-" n="settenario">
					<!-- INPUT: Per-ch'el+la vi de-si+a =d -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ch'el</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">a</seg>
				</l>
				<l rhyme="d" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: O_in e-stre+mo cru-de+le_o_in tut+to pi+a. =D -->
					<seg type="syl" met="-">O<c met="_"> </c>in </seg>
					<seg type="syl" met="-">e</seg>
					<seg type="syl" met="+">stre</seg>
					<seg type="syl" met="-">mo </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">le<c met="_"> </c>o<c met="_"> </c>in </seg>
					<seg type="syl" met="+">tut</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">pi</seg>
					<seg type="syl" met="-">a.</seg>
				</l>
