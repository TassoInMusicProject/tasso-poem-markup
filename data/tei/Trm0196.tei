				</l>
				<l rhyme="f" met="---+-+---+-" n="endecasillabo">
					<!-- INPUT: E_i sus-sur-ran+ti_au-gel+li,_in fra le fron+de =F -->
					<seg type="syl" met="-">E<c met="_"> </c>i </seg>
					<seg type="syl" met="-">sus</seg>
					<seg type="syl" met="-">sur</seg>
					<seg type="syl" met="+">ran</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>au</seg>
					<seg type="syl" met="+">gel</seg>
					<seg type="syl" met="-">li,<c met="_"> </c>in </seg>
					<seg type="syl" met="-">fra </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fron</seg>
					<seg type="syl" met="-">de</seg>
				</l>
				<l rhyme="g" met="---+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Al mor-mo-rar+ de l'on+de:_e va+ghi fio+ri =G+ -->
					<seg type="syl" met="-">Al </seg>
					<seg type="syl" met="-">mor</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">rar </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="+">l'on</seg>
					<seg type="syl" met="-">de:<c met="_"> </c>e </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ghi </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="g" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Do+nan le Gra+zie_a_i par-go-let+ti_A-mo+ri. =G -->
					<seg type="syl" met="+">Do</seg>
					<seg type="syl" met="-">nan </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">Gra</seg>
					<seg type="syl" met="-">zie<c met="_"> </c>a<c met="_"> </c>i </seg>
					<seg type="syl" met="-">par</seg>
					<seg type="syl" met="-">go</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>A</seg>
					<seg type="syl" met="+">mo</seg>
