<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Dolcissimi colori</title>
			</titleStmt>
			<publicationStmt>
				<publisher>Tasso in Music Project</publisher>
				<idno>Trm0266</idno>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---+-" n="settenario" enjamb="yes">
					<!-- INPUT: Dol-cis+si-mi co-lo+ri, =a+ -->
					<seg type="syl" met="-">Dol</seg>
					<seg type="syl" met="+">cis</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ri,</seg>
				</l>
				<l rhyme="b" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Voi vi mu-ta+te,_ed i+o =b+ -->
					<seg type="syl" met="-">Voi </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">mu</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="b" met="+-+------+-" n="endecasillabo">
					<!-- INPUT: Mu+to_a-spet+to con voi, ma non de-si+o. =B -->
					<seg type="syl" met="+">Mu</seg>
					<seg type="syl" met="-">to<c met="_"> </c>a</seg>
					<seg type="syl" met="+">spet</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">voi, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">o.</seg>
				</l>
				<l rhyme="c" met="+--+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Sem+pre vor-rei+ mi-rar+vi,_e se fio-ri+re =C+ -->
					<seg type="syl" met="+">Sem</seg>
					<seg type="syl" met="-">pre </seg>
					<seg type="syl" met="-">vor</seg>
					<seg type="syl" met="+">rei </seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">rar</seg>
					<seg type="syl" met="-">vi,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">fio</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario">
					<!-- INPUT: Un bel pur-pu+reo veg+gio, =d -->
					<seg type="syl" met="-">Un </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="-">pur</seg>
					<seg type="syl" met="+">pu</seg>
					<seg type="syl" met="-">reo </seg>
					<seg type="syl" met="+">veg</seg>
					<seg type="syl" met="-">gio,</seg>
				</l>
				<l rhyme="d" met="--+--++--+-" n="endecasillabo">
					<!-- INPUT: E quel va+go can-dor+ sem+pre_io va-gheg+gio; =D -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="-">can</seg>
					<seg type="syl" met="+">dor </seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pre<c met="_"> </c>io </seg>
					<seg type="syl" met="-">va</seg>
					<seg type="syl" met="+">gheg</seg>
					<seg type="syl" met="-">gio;</seg>
				</l>
				<l rhyme="e" met="--++-+---+-" n="endecasillabo">
					<!-- INPUT: E per-ché+ va+ri se+gno_al mio pen-sie+ro =E -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ri </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>al </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">ro</seg>
				</l>
				<l rhyme="e" met="+-+--+-" n="settenario">
					<!-- INPUT: È+ co-stan+te l'ar-cie+ro. =e -->
					<seg type="syl" met="+">È </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">stan</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">l'ar</seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">ro.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
