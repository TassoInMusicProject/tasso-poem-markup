<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Fummo felici un tempo</title>
			</titleStmt>
			<publicationStmt>
				<publisher>Tasso in Music Project</publisher>
				<idno>Trm0190</idno>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Fum+mo_un tem+po fe-li+ci =a -->
					<seg type="syl" met="+">Fum</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>un </seg>
					<seg type="syl" met="+">tem</seg>
					<seg type="syl" met="-">po </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">li</seg>
					<seg type="syl" met="-">ci</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario">
					<!-- INPUT: Io a-man+te_ed a-ma+to, =b -->
					<seg type="syl" met="-">Io </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te<c met="_"> </c>ed </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Tu a-ma+ta_ed a-man+te_in dol+ce sta+to. =B -->
					<seg type="syl" met="-">Tu </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>ed </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te<c met="_"> </c>in </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">sta</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="c" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Poi d'a-man+te ne-mi+ca =c+ -->
					<seg type="syl" met="-">Poi </seg>
					<seg type="syl" met="-">d'a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">ne</seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">ca</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Voi di-ve-ni+ste,_ed i+o =d+ -->
					<seg type="syl" met="-">Voi </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">ni</seg>
					<seg type="syl" met="-">ste,<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Vol+si_in di-sde+gno_il gio-ve-nil+ de-si+o. =D -->
					<seg type="syl" met="+">Vol</seg>
					<seg type="syl" met="-">si<c met="_"> </c>in </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>il </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">nil </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">o.</seg>
				</l>
				<l rhyme="c" met="+----+-" n="settenario">
					<!-- INPUT: Sde+gno vuol ch'io ve_'l di+ca, =c -->
					<seg type="syl" met="+">Sde</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="-">vuol </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="-">ve<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">ca,</seg>
				</l>
				<l rhyme="b" met="+----+-" n="settenario">
					<!-- INPUT: Sde+gno che nel mio pet+to =b -->
					<seg type="syl" met="+">Sde</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="-+-+-----+-" n="endecasillabo">
					<!-- INPUT: Tien vi+va l'on+ta del mio duol ne-glet+to; =B -->
					<seg type="syl" met="-">Tien </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">va </seg>
					<seg type="syl" met="+">l'on</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">duol </seg>
					<seg type="syl" met="-">ne</seg>
					<seg type="syl" met="+">glet</seg>
					<seg type="syl" met="-">to;</seg>
				</l>
				<l rhyme="e" met="--+--+-" n="settenario">
					<!-- INPUT: E le fron+de ne svel+le =e -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">fron</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">svel</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="e" met="-+-+-+--++-" n="endecasillabo">
					<!-- INPUT: Del vo+stro lau+ro,_or sec+che_e già sì+ bel+le. =E -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="+">lau</seg>
					<seg type="syl" met="-">ro,<c met="_"> </c>or </seg>
					<seg type="syl" met="+">sec</seg>
					<seg type="syl" met="-">che<c met="_"> </c>e </seg>
					<seg type="syl" met="-">già </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">le.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
