	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Cre+di dun+que tu, scioc+co, che mai Daf+ne =A+ -->
					<seg type="syl" met="+">Cre</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">dun</seg>
					<seg type="syl" met="-">que </seg>
					<seg type="syl" met="-">tu, </seg>
					<seg type="syl" met="+">scioc</seg>
					<seg type="syl" met="-">co, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">mai </seg>
					<seg type="syl" met="+">Daf</seg>
					<seg type="syl" met="-">ne</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Con-si-glias+se l'an-dar,+ se non ve-des+se =B+ -->
					<seg type="syl" met="-">Con</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">glias</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">l'an</seg>
					<seg type="syl" met="+">dar, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">des</seg>
					<seg type="syl" met="-">se</seg>
				</l>
				<l rhyme="c" met="-+---+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: In par+te_il cor di Sil+via?_E for+se ch'el+la =C+ -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="-">cor </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">Sil</seg>
					<seg type="syl" met="-">via?<c met="_"> </c>E </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="+">ch'el</seg>
					<seg type="syl" met="-">la</seg>
				</l>
				<l rhyme="d" met="--+--++--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Il sa, nè+ vuol pe-rò+ ch'al+tri ri-sap+pia =D+ -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="-">sa, </seg>
					<seg type="syl" met="+">nè </seg>
					<seg type="syl" met="-">vuol </seg>
					<seg type="syl" met="-">pe</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="+">ch'al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">sap</seg>
					<seg type="syl" met="-">pia</seg>
				</l>
				<l rhyme="e" met="+-++---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ch'el+la ciò+ sap+pia._Hor, se_'l con-sen+so_esp-res+so =E+ -->
					<seg type="syl" met="+">Ch'el</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">ciò </seg>
					<seg type="syl" met="+">sap</seg>
					<seg type="syl" met="-">pia.<c met="_"> </c>Hor, </seg>
					<seg type="syl" met="-">se<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">sen</seg>
					<seg type="syl" met="-">so<c met="_"> </c>esp</seg>
					<seg type="syl" met="+">res</seg>
					<seg type="syl" met="-">so</seg>
				</l>
				<l rhyme="f" met="+----+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Cer+chi di lei, non ve+di che tu cer+chi =F+ -->
					<seg type="syl" met="+">Cer</seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">lei, </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="+">cer</seg>
					<seg type="syl" met="-">chi</seg>
				</l>
				<l rhyme="g" met="+--+-+-+++-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Quel+lo che più+ le pia+ce?_Hor do+v'è+ dun+que =G+ -->
					<seg type="syl" met="+">Quel</seg>
					<seg type="syl" met="-">lo </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">pia</seg>
					<seg type="syl" met="-">ce?<c met="_"> </c>Hor </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="+">v'è </seg>
					<seg type="syl" met="+">dun</seg>
					<seg type="syl" met="-">que</seg>
				</l>
				<l rhyme="h" met="+----+---+-" n="endecasillabo">
					<!-- INPUT: Que+sto tuo de-si-de+rio di pia-cer+le? =H -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">rio </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">pia</seg>
					<seg type="syl" met="+">cer</seg>
					<seg type="syl" met="-">le?</seg>
				</l>
			</lg>
		</body>
	</text>
