<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-------+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ah, ma non fia che fra tan-t’ar+mi_e tan+te =A+ -->
					<seg type="syl" met="-">Ah, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">fia </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">fra </seg>
					<seg type="syl" met="-">tan</seg>
					<seg type="syl" met="+">t’ar</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>e </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="+--++--+-+-" n="endecasillabo">
					<!-- INPUT: U+na di san+gue_og+gi si ba+gni_al-me+no? =B -->
					<seg type="syl" met="+">U</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">san</seg>
					<seg type="syl" met="+">gue<c met="_"> </c>og</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="+">ba</seg>
					<seg type="syl" met="-">gni<c met="_"> </c>al</seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">no?</seg>
				</l>
				<l rhyme="a" met="++-+-----+-" n="endecasillabo">
					<!-- INPUT: S’o+gni_al+tro pet+to_a voi par di dia-man+te, =A -->
					<seg type="syl" met="+">S’o</seg>
					<seg type="syl" met="+">gni<c met="_"> </c>al</seg>
					<seg type="syl" met="-">tro </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to<c met="_"> </c>a </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">par </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">dia</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="b" met="--+--+--++-" n="endecasillabo">
					<!-- INPUT: O-sa-re+te pia-gar+ fem-mi-nil+ se+no; =B -->
					<seg type="syl" met="-">O</seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">pia</seg>
					<seg type="syl" met="+">gar </seg>
					<seg type="syl" met="-">fem</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="+">nil </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">no;</seg>
				</l>
				<l rhyme="a" met="-+-----+-+-" n="endecasillabo">
					<!-- INPUT: In que+sto mio, che vi sta nu+do_a-van+te, =A -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="-">mio, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="+">nu</seg>
					<seg type="syl" met="-">do<c met="_"> </c>a</seg>
					<seg type="syl" met="+">van</seg>
					<seg type="syl" met="-">te,</seg>
				</l>
				<l rhyme="b" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: I pre+gj vos+tri_e le vit-to+rie sie+no. =B -->
					<seg type="syl" met="-">I </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">gj </seg>
					<seg type="syl" met="+">vos</seg>
					<seg type="syl" met="-">tri<c met="_"> </c>e </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">vit</seg>
					<seg type="syl" met="+">to</seg>
					<seg type="syl" met="-">rie </seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
				<l rhyme="c" met="+--+-+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Te+ne-ro_ai col+pi_è que+sto mio; ben sal+lo =C+ -->
					<seg type="syl" met="+">Te</seg>
					<seg type="syl" met="-">ne</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>ai </seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">pi<c met="_"> </c>è </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="-">mio; </seg>
					<seg type="syl" met="-">ben </seg>
					<seg type="syl" met="+">sal</seg>
					<seg type="syl" met="-">lo</seg>
				</l>
				<l rhyme="c" met="-+-----+-+-" n="endecasillabo">
					<!-- INPUT: A-mor,+ che mai non vi sa-et+ta_in fal+lo. =C -->
					<seg type="syl" met="-">A</seg>
					<seg type="syl" met="+">mor, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">mai </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">et</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>in </seg>
					<seg type="syl" met="+">fal</seg>
					<seg type="syl" met="-">lo.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
