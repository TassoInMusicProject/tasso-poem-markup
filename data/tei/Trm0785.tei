	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Por+ta-no l'al+tre_il ve+lo; =a -->
					<seg type="syl" met="+">Por</seg>
					<seg type="syl" met="-">ta</seg>
					<seg type="syl" met="-">no </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">tre<c met="_"> </c>il </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">lo;</seg>
				</l>
				<l rhyme="b" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Voi+ le chio+me do-ra+te =b+ -->
					<seg type="syl" met="+">Voi </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">chio</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="b" met="+----+-+-+-" n="endecasillabo">
					<!-- INPUT: For+se per al-te-rez+za_al sol+ mo-stra+te. =B -->
					<seg type="syl" met="+">For</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">al</seg>
					<seg type="syl" met="-">te</seg>
					<seg type="syl" met="+">rez</seg>
					<seg type="syl" met="-">za<c met="_"> </c>al </seg>
					<seg type="syl" met="+">sol </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">stra</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
				<l rhyme="b" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Ma s'a sde+gno pren-de+te =b+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">s'a </seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="-">pren</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="c" met="+-+--+-" n="settenario">
					<!-- INPUT: O+gni_e-sem+pio ter-re+no, =c -->
					<seg type="syl" met="+">O</seg>
					<seg type="syl" met="-">gni<c met="_"> </c>e</seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pio </seg>
					<seg type="syl" met="-">ter</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Con al+ti_e-sem+pi_il ciel+ vi muo+va_al-me+no: =C -->
					<seg type="syl" met="-">Con </seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>e</seg>
					<seg type="syl" met="+">sem</seg>
					<seg type="syl" met="-">pi<c met="_"> </c>il </seg>
					<seg type="syl" met="+">ciel </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">muo</seg>
					<seg type="syl" met="-">va<c met="_"> </c>al</seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">no:</seg>
				</l>
				<l rhyme="b" met="-++--+-" n="settenario">
					<!-- INPUT: Col vel+ l'Al+ba ve-de+te =b -->
					<seg type="syl" met="-">Col </seg>
					<seg type="syl" met="+">vel </seg>
					<seg type="syl" met="+">l'Al</seg>
					<seg type="syl" met="-">ba </seg>
					<seg type="syl" met="-">ve</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="a" met="-+-+-+-" n="settenario">
					<!-- INPUT: E lei+ che nac+que_in De+lo, =a -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">lei </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">nac</seg>
					<seg type="syl" met="-">que<c met="_"> </c>in </seg>
					<seg type="syl" met="+">De</seg>
					<seg type="syl" met="-">lo,</seg>
				</l>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: E l'I+ri_il suo+ co-lo+ra_an-co+ nel cie+lo. =A -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">l'I</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>il </seg>
					<seg type="syl" met="+">suo </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>an</seg>
					<seg type="syl" met="+">co </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="-">lo.</seg>
				</l>
			</lg>
		</body>
	</text>
