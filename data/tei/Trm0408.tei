	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Que+sto tra gli_al+tri fio+ri =a+ -->
					<seg type="syl" met="+">Que</seg>
					<seg type="syl" met="-">sto </seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>al</seg>
					<seg type="syl" met="-">tri </seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="b" met="-+-+-+-" n="settenario">
					<!-- INPUT: San-gui+gno sì+ fu tin+to =b -->
					<seg type="syl" met="-">San</seg>
					<seg type="syl" met="+">gui</seg>
					<seg type="syl" met="-">gno </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">fu </seg>
					<seg type="syl" met="+">tin</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Del bel san+gue d'A-do+ne_o di Gia-cin+to? =B -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="-">bel </seg>
					<seg type="syl" met="+">san</seg>
					<seg type="syl" met="-">gue </seg>
					<seg type="syl" met="-">d'A</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>o </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="-">Gia</seg>
					<seg type="syl" met="+">cin</seg>
					<seg type="syl" met="-">to?</seg>
				</l>
				<l rhyme="c" met="-----+-" n="settenario" enjamb="yes">
					<!-- INPUT: O pur in lui con-ver+se =c+ -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">in </seg>
					<seg type="syl" met="-">lui </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">se</seg>
				</l>
				<l rhyme="d" met="---+-+-" n="settenario">
					<!-- INPUT: Il suo l'a-la+to_id-di+o, =d -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="-">l'a</seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">to<c met="_"> </c>id</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">o,</seg>
				</l>
				<l rhyme="d" met="+----+---+-" n="endecasillabo">
					<!-- INPUT: Quan+do_un suo stral gli cad+de_e si fe-rì+o? =D -->
					<seg type="syl" met="+">Quan</seg>
					<seg type="syl" met="-">do<c met="_"> </c>un </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="-">stral </seg>
					<seg type="syl" met="-">gli </seg>
					<seg type="syl" met="+">cad</seg>
					<seg type="syl" met="-">de<c met="_"> </c>e </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">rì</seg>
					<seg type="syl" met="-">o?</seg>
				</l>
				<l rhyme="e" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Deh!+ sa-rà+ mai ch'io ver+si =e+ -->
					<seg type="syl" met="+">Deh! </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="-">mai </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">si</seg>
				</l>
				<l rhyme="a" met="+--+-+-" n="settenario">
					<!-- INPUT: Sì+ for-tu-na+ti_u-mo+ri =a -->
					<seg type="syl" met="+">Sì </seg>
					<seg type="syl" met="-">for</seg>
					<seg type="syl" met="-">tu</seg>
					<seg type="syl" met="+">na</seg>
					<seg type="syl" met="-">ti<c met="_"> </c>u</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">ri</seg>
				</l>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Ch'ei sì+ li can+gi_e_'l vo+stro se+no_in-fio+ri? =A -->
					<seg type="syl" met="-">Ch'ei </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">li </seg>
					<seg type="syl" met="+">can</seg>
					<seg type="syl" met="-">gi<c met="_"> </c>e<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">no<c met="_"> </c>in</seg>
					<seg type="syl" met="+">fio</seg>
					<seg type="syl" met="-">ri?</seg>
				</l>
			</lg>
		</body>
