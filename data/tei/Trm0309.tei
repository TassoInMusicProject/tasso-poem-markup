	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: O ver+di sel+ve,_o dol+ci fon+ti,_o ri+vi, =A -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">ver</seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">sel</seg>
					<seg type="syl" met="-">ve,<c met="_"> </c>o </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">fon</seg>
					<seg type="syl" met="-">ti,<c met="_"> </c>o </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">vi,</seg>
				</l>
				<l rhyme="b" met="-++--+-" n="settenario">
					<!-- INPUT: O luo+ghi_er+mi_e sel-vag+gi, =b -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">luo</seg>
					<seg type="syl" met="+">ghi<c met="_"> </c>er</seg>
					<seg type="syl" met="-">mi<c met="_"> </c>e </seg>
					<seg type="syl" met="-">sel</seg>
					<seg type="syl" met="+">vag</seg>
					<seg type="syl" met="-">gi,</seg>
				</l>
				<l rhyme="b" met="+-+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Pi+ni,_a-be+ti, gi-ne+pri,_al-lo+ri_e fag+gi; =B -->
					<seg type="syl" met="+">Pi</seg>
					<seg type="syl" met="-">ni,<c met="_"> </c>a</seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="-">ti, </seg>
					<seg type="syl" met="-">gi</seg>
					<seg type="syl" met="+">ne</seg>
					<seg type="syl" met="-">pri,<c met="_"> </c>al</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>e </seg>
					<seg type="syl" met="+">fag</seg>
					<seg type="syl" met="-">gi;</seg>
				</l>
				<l rhyme="a" met="-+-+--+--+-" n="endecasillabo">
					<!-- INPUT: O va+ghi_au-gel+li, sem-pli+ci_e la-sci+vi, =A -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ghi<c met="_"> </c>au</seg>
					<seg type="syl" met="+">gel</seg>
					<seg type="syl" met="-">li, </seg>
					<seg type="syl" met="-">sem</seg>
					<seg type="syl" met="+">pli</seg>
					<seg type="syl" met="-">ci<c met="_"> </c>e </seg>
					<seg type="syl" met="-">la</seg>
					<seg type="syl" met="+">sci</seg>
					<seg type="syl" met="-">vi,</seg>
				</l>
				<l rhyme="c" met="+----+---+-" n="endecasillabo">
					<!-- INPUT: E+co,_e tu che ri-spon+di_al mio la-men+to, =C -->
					<seg type="syl" met="+">E</seg>
					<seg type="syl" met="-">co,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">spon</seg>
					<seg type="syl" met="-">di<c met="_"> </c>al </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">la</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="d" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Chi può+ dar fi+ne_a sì+ cru-del+ for-tu+na? =D -->
					<seg type="syl" met="-">Chi </seg>
					<seg type="syl" met="+">può </seg>
					<seg type="syl" met="-">dar </seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>a </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">del </seg>
					<seg type="syl" met="-">for</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">na?</seg>
				</l>
				<l rhyme="d" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: "<i>U+na</i>." Dun+que sol u+na =d+ -->
					<seg type="syl" met="+">"&lt;i&gt;U</seg>
					<seg type="syl" met="-">na&lt;/i&gt;." </seg>
					<seg type="syl" met="+">Dun</seg>
					<seg type="syl" met="-">que </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="+">u</seg>
					<seg type="syl" met="-">na</seg>
				</l>
				<l rhyme="c" met="---+--+--+-" n="endecasillabo">
					<!-- INPUT: E fa co-sì+ la-cri-me+vol con-cen+to? =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">fa </seg>
					<seg type="syl" met="-">co</seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="-">la</seg>
					<seg type="syl" met="-">cri</seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">vol </seg>
					<seg type="syl" met="-">con</seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">to?</seg>
				</l>
				<l rhyme="e" met="+---++---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: "<i>Cen+to</i>." Non son già+ cen+to,_e pur son mol+te =E+ -->
					<seg type="syl" met="+">"&lt;i&gt;Cen</seg>
					<seg type="syl" met="-">to&lt;/i&gt;." </seg>
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">già </seg>
					<seg type="syl" met="+">cen</seg>
					<seg type="syl" met="-">to,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">mol</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="e" met="-+-+-+-" n="settenario">
					<!-- INPUT: In bel+la fe+sta_ac-col+te: =e -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">sta<c met="_"> </c>ac</seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">te:</seg>
				</l>
				<l rhyme="f" met="++--++---+-" n="endecasillabo">
					<!-- INPUT: Co+me_u+na po-trà+ dun+que_il mal for-ni+re? =F -->
					<seg type="syl" met="+">Co</seg>
					<seg type="syl" met="+">me<c met="_"> </c>u</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="-">po</seg>
					<seg type="syl" met="+">trà </seg>
					<seg type="syl" met="+">dun</seg>
					<seg type="syl" met="-">que<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mal </seg>
					<seg type="syl" met="-">for</seg>
					<seg type="syl" met="+">ni</seg>
					<seg type="syl" met="-">re?</seg>
				</l>
				<l rhyme="c" met="+--+--+--+-" n="endecasillabo">
					<!-- INPUT: "<i>I+re</i>," Per i+ra mia né+ per di-spet+to =C -->
					<seg type="syl" met="+">"&lt;i&gt;I</seg>
					<seg type="syl" met="-">re&lt;/i&gt;," </seg>
					<seg type="syl" met="-">Per </seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">mia </seg>
					<seg type="syl" met="+">né </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">spet</seg>
					<seg type="syl" met="-">to</seg>
				</l>
				<l rhyme="c" met="--++-+-+-+-" n="endecasillabo">
					<!-- INPUT: Non av-rà+ fi+ne_a-mor+ nel no+stro pet+to? =C -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">av</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="+">fi</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>a</seg>
					<seg type="syl" met="+">mor </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">stro </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to?</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
