<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<?xml-model href="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="application/xml" schematypens="http://purl.oclc.org/dsdl/schematron"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>Tasso in Music Project: verse analytical encoding sample</title>
			</titleStmt>
			<publicationStmt>
				<p>For Emiliano and Craig</p>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-----+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Sia que+sta pur tra le mie fro+di,_e va+glia =A+ -->
					<seg type="syl" met="-">Sia </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sta </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">mie </seg>
					<seg type="syl" met="+">fro</seg>
					<seg type="syl" met="-">di,<c met="_"> </c>e </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">glia</seg>
				</l>
				<l rhyme="b" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Sì di tan+te mie col+pe_in te_il di-fet+to, =B -->
					<seg type="syl" met="-">Sì </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">mie </seg>
					<seg type="syl" met="+">col</seg>
					<seg type="syl" met="-">pe<c met="_"> </c>in </seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">fet</seg>
					<seg type="syl" met="-">to,</seg>
				</l>
				<l rhyme="a" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: Che tu quin+ci ti par+ta,_e non ti ca+glia =A -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="+">quin</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">ta,<c met="_"> </c>e </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">glia</seg>
				</l>
				<l rhyme="b" met="-+-+-----+-" n="endecasillabo">
					<!-- INPUT: Di que+sto_al-ber+go tuo già sì di-let+to. =B -->
					<seg type="syl" met="-">Di </seg>
					<seg type="syl" met="+">que</seg>
					<seg type="syl" met="-">sto<c met="_"> </c>al</seg>
					<seg type="syl" met="+">ber</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="-">tuo </seg>
					<seg type="syl" met="-">già </seg>
					<seg type="syl" met="-">sì </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">let</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="a" met="+--+--+--+-" n="endecasillabo">
					<!-- INPUT: Vat+te-ne: pas+sa_il mar: pu+gna, tra-va+glia: =A -->
					<seg type="syl" met="+">Vat</seg>
					<seg type="syl" met="-">te</seg>
					<seg type="syl" met="-">ne: </seg>
					<seg type="syl" met="+">pas</seg>
					<seg type="syl" met="-">sa<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mar: </seg>
					<seg type="syl" met="+">pu</seg>
					<seg type="syl" met="-">gna, </seg>
					<seg type="syl" met="-">tra</seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">glia:</seg>
				</l>
				<l rhyme="b" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Strug+gi la fe+de nos+tra;_an-ch’io+ t’af-fret+to. =B -->
					<seg type="syl" met="+">Strug</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="+">nos</seg>
					<seg type="syl" met="-">tra;<c met="_"> </c>an</seg>
					<seg type="syl" met="+">ch’io </seg>
					<seg type="syl" met="-">t’af</seg>
					<seg type="syl" met="+">fret</seg>
					<seg type="syl" met="-">to.</seg>
				</l>
				<l rhyme="c" met="-+-+-----+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Che di+co nos+tra?_ah non più mia; fe-de+le =C+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">co </seg>
					<seg type="syl" met="+">nos</seg>
					<seg type="syl" met="-">tra?<c met="_"> </c>ah </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="-">mia; </seg>
					<seg type="syl" met="-">fe</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="c" met="+--++----+-" n="endecasillabo">
					<!-- INPUT: So+no_a te so+lo,_i+do-lo mio cru-de+le. =C -->
					<seg type="syl" met="+">So</seg>
					<seg type="syl" met="-">no<c met="_"> </c>a </seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="+">lo,<c met="_"> </c>i</seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="-">lo </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">cru</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">le.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
