	<text>
		<body>
			<lg>
				<l rhyme="a" met="--+--+-" n="settenario">
					<!-- INPUT: So-a-vis+si-mo ba+cio, =a -->
					<seg type="syl" met="-">So</seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">vis</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">mo </seg>
					<seg type="syl" met="+">ba</seg>
					<seg type="syl" met="-">cio,</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Del mio lun+go ser-vir+ con tan+ta fe+de =B -->
					<seg type="syl" met="-">Del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="+">lun</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="-">ser</seg>
					<seg type="syl" met="+">vir </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="+">fe</seg>
					<seg type="syl" met="-">de</seg>
				</l>
				<l rhyme="b" met="-+---+-" n="settenario">
					<!-- INPUT: Dol-cis+si-ma mer-ce+de! =b -->
					<seg type="syl" met="-">Dol</seg>
					<seg type="syl" met="+">cis</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">mer</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">de!</seg>
				</l>
				<l rhyme="c" met="--+--+-" n="settenario">
					<!-- INPUT: Fe-li-cis+si-mo_ar-di+re =c -->
					<seg type="syl" met="-">Fe</seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="+">cis</seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="d" met="-----+-" n="settenario" enjamb="yes">
					<!-- INPUT: De la man che vi toc+ca =d+ -->
					<seg type="syl" met="-">De </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">man </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">vi </seg>
					<seg type="syl" met="+">toc</seg>
					<seg type="syl" met="-">ca</seg>
				</l>
				<l rhyme="e" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Tut+ta tre-man+te_il de-li-ca+to se+no. =E -->
					<seg type="syl" met="+">Tut</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">tre</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="-">li</seg>
					<seg type="syl" met="+">ca</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">no.</seg>
				</l>
				<l rhyme="d" met="+--+-+-" n="settenario">
					<!-- INPUT: Men+tre di boc+ca_in boc+ca =d -->
					<seg type="syl" met="+">Men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">di </seg>
					<seg type="syl" met="+">boc</seg>
					<seg type="syl" met="-">ca<c met="_"> </c>in </seg>
					<seg type="syl" met="+">boc</seg>
					<seg type="syl" met="-">ca</seg>
				</l>
				<l rhyme="e" met="+----+-+-+-" n="endecasillabo">
					<!-- INPUT: L'a+ni-ma per dol-cez+za_al-lor+ vien me+no! =E -->
					<seg type="syl" met="+">L'a</seg>
					<seg type="syl" met="-">ni</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">dol</seg>
					<seg type="syl" met="+">cez</seg>
					<seg type="syl" met="-">za<c met="_"> </c>al</seg>
					<seg type="syl" met="+">lor </seg>
					<seg type="syl" met="-">vien </seg>
					<seg type="syl" met="+">me</seg>
					<seg type="syl" met="-">no!</seg>
				</l>
			</lg>
		</body>
	</text>
