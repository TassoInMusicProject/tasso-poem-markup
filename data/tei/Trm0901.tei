	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+---+-" n="settenario">
					<!-- INPUT: O ti+mi-da lep-ret+ta, =a -->
					<seg type="syl" met="-">O </seg>
					<seg type="syl" met="+">ti</seg>
					<seg type="syl" met="-">mi</seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">lep</seg>
					<seg type="syl" met="+">ret</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="a" met="-+-+---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Che men+tre fug+gi per sal-var+ la vi+ta, =A+ -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="+">fug</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">sal</seg>
					<seg type="syl" met="+">var </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="a" met="+-+--+++-+-" n="endecasillabo">
					<!-- INPUT: Giun+gi do+ve la mor+te_è+ più+ gra-di+ta; =A -->
					<seg type="syl" met="+">Giun</seg>
					<seg type="syl" met="-">gi </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="+">te<c met="_"> </c>è </seg>
					<seg type="syl" met="+">più </seg>
					<seg type="syl" met="-">gra</seg>
					<seg type="syl" met="+">di</seg>
					<seg type="syl" met="-">ta;</seg>
				</l>
				<l rhyme="b" met="-+-+++-" n="settenario">
					<!-- INPUT: S'in-nan+zi_a sì+ be+gli_oc+chi, =b -->
					<seg type="syl" met="-">S'in</seg>
					<seg type="syl" met="+">nan</seg>
					<seg type="syl" met="-">zi<c met="_"> </c>a </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">be</seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi,</seg>
				</l>
				<l rhyme="b" met="++-+---+-+-" n="endecasillabo">
					<!-- INPUT: Là+ do+ve_io pre+go che_'l mo-rir+ mi toc+chi, =B -->
					<seg type="syl" met="+">Là </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>io </seg>
					<seg type="syl" met="+">pre</seg>
					<seg type="syl" met="-">go </seg>
					<seg type="syl" met="-">che<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">toc</seg>
					<seg type="syl" met="-">chi,</seg>
				</l>
				<l rhyme="c" met="--+--+-" n="settenario">
					<!-- INPUT: Il mo-rir+ ti di-spia+ce, =c -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">rir </seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">spia</seg>
					<seg type="syl" met="-">ce,</seg>
				</l>
				<l rhyme="c" met="--+--+-+-+-" n="endecasillabo">
					<!-- INPUT: Non sai co+me ri-po+so_ap-por+ta_e pa+ce. =C -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">sai </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">po</seg>
					<seg type="syl" met="-">so<c met="_"> </c>ap</seg>
					<seg type="syl" met="+">por</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>e </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">ce.</seg>
				</l>
