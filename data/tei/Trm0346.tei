					<seg type="syl" met="+">fiu</seg>
					<seg type="syl" met="-">me</seg>
				</l>
				<l rhyme="b" met="+----+-" n="settenario">
					<!-- INPUT: Tir+si d'E-le-o-no+ra, =b -->
					<seg type="syl" met="+">Tir</seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="-">d'E</seg>
					<seg type="syl" met="-">le</seg>
					<seg type="syl" met="-">o</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ra,</seg>
				</l>
				<l rhyme="b" met="--+--+-+-+--" n="12">
					<!-- INPUT: E ri-spon+dean le sel+ve_e l'on+de_&mdash;_o-no+ra. &mdash; =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">spon</seg>
					<seg type="syl" met="-">dean </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">sel</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>e </seg>
					<seg type="syl" met="+">l'on</seg>
					<seg type="syl" met="-">de<c met="_"> </c>&amp;mdash;<c met="_"> </c>o</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ra. </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="c" met="--+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: E pa-rea+ mor-mo-ran+do =c+ -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">pa</seg>
					<seg type="syl" met="+">rea </seg>
					<seg type="syl" met="-">mor</seg>
					<seg type="syl" met="-">mo</seg>
					<seg type="syl" met="+">ran</seg>
					<seg type="syl" met="-">do</seg>
				</l>
				<l rhyme="d" met="-++--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Dir l'ò+ra:_&mdash;_ó+ra, ch'ap-pa+re, =d+ -->
					<seg type="syl" met="-">Dir </seg>
					<seg type="syl" met="+">l'ò</seg>
					<seg type="syl" met="+">ra:<c met="_"> </c>&amp;mdash;<c met="_"> </c>ó</seg>
					<seg type="syl" met="-">ra, </seg>
					<seg type="syl" met="-">ch'ap</seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="d" met="-+---++--+-" n="endecasillabo">
					<!-- INPUT: L'au-ro+ra par che lie+ta_e+sca del ma+re: =D -->
					<seg type="syl" met="-">L'au</seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="-">par </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">lie</seg>
					<seg type="syl" met="+">ta<c met="_"> </c>e</seg>
					<seg type="syl" met="-">sca </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">re:</seg>
				</l>
				<l rhyme="c" met="---+-+--" n="8">
					<!-- INPUT: Or chi l'o-no+ra_a-man+do? &mdash; =c -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">l'o</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ra<c met="_"> </c>a</seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">do? </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="e" met="-+-+-+--" n="8">
					<!-- INPUT: E l'ac+que_in-sie+me_e_i ra+mi: &mdash; =e -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">l'ac</seg>
					<seg type="syl" met="-">que<c met="_"> </c>in</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">me<c met="_"> </c>e<c met="_"> </c>i </seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">mi: </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
				<l rhyme="e" met="-----+---+--" n="12">
					<!-- INPUT: Or chi fia che l'o-no+ri_e che non l'a+mi? &mdash; =E -->
					<seg type="syl" met="-">Or </seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">fia </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">l'o</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>e </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">l'a</seg>
					<seg type="syl" met="-">mi? </seg>
					<seg type="syl" met="-">&amp;mdash;</seg>
				</l>
