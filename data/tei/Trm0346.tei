	<text>
		<body>
			<lg>
				<l rhyme="a" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Can-ta+va_in ri+va_al fiu+me =a+ -->
					<seg type="syl" met="-">Can</seg>
					<seg type="syl" met="+">ta</seg>
					<seg type="syl" met="-">va<c met="_"> </c>in </seg>
					<seg type="syl" met="+">ri</seg>
