<TEI xmlns="http://www.tei-c.org/ns/1.0">
	<teiHeader>
		<fileDesc>
			<titleStmt>
				<title>A chi creder degg'io</title>
			</titleStmt>
			<publicationStmt>
				<publisher>Tasso in Music Project</publisher>
				<idno>Trm0097</idno>
			</publicationStmt>
			<sourceDesc>
				<p>Born digital with examples from Tasso in Music Project</p>
			</sourceDesc>
		</fileDesc>
		<encodingDesc>
			<metDecl type="met">
				<metSym value="+">stressed syllable</metSym>
				<metSym value="-">unstressed syllable</metSym>
				<metSym value="_">elision (synalepha?)</metSym>
			</metDecl>
		</encodingDesc>
	</teiHeader>
	<text>
		<body>
			<lg>
				<l rhyme="a" met="---+-+-" n="settenario">
					<!-- INPUT: A chi cre-der+ deg-g'i+o =a -->
					<seg type="syl" met="-">A </seg>
					<seg type="syl" met="-">chi </seg>
					<seg type="syl" met="-">cre</seg>
					<seg type="syl" met="+">der </seg>
					<seg type="syl" met="-">deg</seg>
					<seg type="syl" met="+">g'i</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="b" met="-+-+-+-" n="settenario">
					<!-- INPUT: Se va+ni so+no_i det+ti =b -->
					<seg type="syl" met="-">Se </seg>
					<seg type="syl" met="+">va</seg>
					<seg type="syl" met="-">ni </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">no<c met="_"> </c>i </seg>
					<seg type="syl" met="+">det</seg>
					<seg type="syl" met="-">ti</seg>
				</l>
				<l rhyme="c" met="-+---+---+-" n="endecasillabo">
					<!-- INPUT: E_'l ven+to se ne por+ta le pa-ro+le? =C -->
					<seg type="syl" met="-">E<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">por</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="-">pa</seg>
					<seg type="syl" met="+">ro</seg>
					<seg type="syl" met="-">le?</seg>
				</l>
				<l rhyme="c" met="---+-+-" n="settenario">
					<!-- INPUT: Non a le vo+ci so+le =c -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="-">a </seg>
					<seg type="syl" met="-">le </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">ci </seg>
					<seg type="syl" met="+">so</seg>
					<seg type="syl" met="-">le</seg>
				</l>
				<l rhyme="b" met="---+---+-+-" n="endecasillabo">
					<!-- INPUT: Che scom-pa-gna+te sian da ve+ri_ef-fet+ti, =B -->
					<seg type="syl" met="-">Che </seg>
					<seg type="syl" met="-">scom</seg>
					<seg type="syl" met="-">pa</seg>
					<seg type="syl" met="+">gna</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">sian </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">ri<c met="_"> </c>ef</seg>
					<seg type="syl" met="+">fet</seg>
					<seg type="syl" met="-">ti,</seg>
				</l>
				<l rhyme="d" met="-+--++-" n="settenario">
					<!-- INPUT: A-mor,+ cre-de-rò+ ma+i; =d -->
					<seg type="syl" met="-">A</seg>
					<seg type="syl" met="+">mor, </seg>
					<seg type="syl" met="-">cre</seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">rò </seg>
					<seg type="syl" met="+">ma</seg>
					<seg type="syl" met="-">i;</seg>
				</l>
				<l rhyme="d" met="-+-+-+---++" n="endecasillabo">
					<!-- INPUT: Ma tan+to_or te+mo, quan+to già spe-ra+i.+ =D -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to<c met="_"> </c>or </seg>
					<seg type="syl" met="+">te</seg>
					<seg type="syl" met="-">mo, </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="-">già </seg>
					<seg type="syl" met="-">spe</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="+">i.</seg>
				</l>
				<l rhyme="e" met="-+---+-" n="settenario">
					<!-- INPUT: A-mor,+ se vuoi ch'io cre+da, =e -->
					<seg type="syl" met="-">A</seg>
					<seg type="syl" met="+">mor, </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">vuoi </seg>
					<seg type="syl" met="-">ch'io </seg>
					<seg type="syl" met="+">cre</seg>
					<seg type="syl" met="-">da,</seg>
				</l>
				<l rhyme="e" met="-+-++--+-+-" n="endecasillabo">
					<!-- INPUT: Con-vien+ che_'l co+re_al+trui ne' fat+ti ve+da. =E -->
					<seg type="syl" met="-">Con</seg>
					<seg type="syl" met="+">vien </seg>
					<seg type="syl" met="-">che<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="+">re<c met="_"> </c>al</seg>
					<seg type="syl" met="-">trui </seg>
					<seg type="syl" met="-">ne' </seg>
					<seg type="syl" met="+">fat</seg>
					<seg type="syl" met="-">ti </seg>
					<seg type="syl" met="+">ve</seg>
					<seg type="syl" met="-">da.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
