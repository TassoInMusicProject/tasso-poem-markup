	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario" enjamb="yes">
					<!-- INPUT: Don+na, quel+la sa-et+ta, =a+ -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na, </seg>
					<seg type="syl" met="+">quel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">sa</seg>
					<seg type="syl" met="+">et</seg>
					<seg type="syl" met="-">ta,</seg>
				</l>
				<l rhyme="b" met="+-+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: On+de già+ mi per-cos+se_il mio si-gno+re, =B+ -->
					<seg type="syl" met="+">On</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="+">già </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="-">per</seg>
					<seg type="syl" met="+">cos</seg>
					<seg type="syl" met="-">se<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">si</seg>
					<seg type="syl" met="+">gno</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="b" met="-+---+-+-+-" n="endecasillabo">
					<!-- INPUT: Ac-ce+se_il mio vo-ler+ d'im-men+so_ar-do+re. =B -->
					<seg type="syl" met="-">Ac</seg>
					<seg type="syl" met="+">ce</seg>
					<seg type="syl" met="-">se<c met="_"> </c>il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">vo</seg>
					<seg type="syl" met="+">ler </seg>
					<seg type="syl" met="-">d'im</seg>
					<seg type="syl" met="+">men</seg>
					<seg type="syl" met="-">so<c met="_"> </c>ar</seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
			</lg>
			<lg>
				<l rhyme="c" met="--++---+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Or, ben-ché+ spen+ta sia nel pet+to mi+o =C+ -->
					<seg type="syl" met="-">Or, </seg>
					<seg type="syl" met="-">ben</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="+">spen</seg>
					<seg type="syl" met="-">ta </seg>
					<seg type="syl" met="-">sia </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="d" met="-+-+---+-+-" n="endecasillabo">
					<!-- INPUT: La bra+ma_e_'l fo+co, pur'-i bra+mo_ed ar+do =D -->
					<seg type="syl" met="-">La </seg>
					<seg type="syl" met="+">bra</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>e<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">fo</seg>
					<seg type="syl" met="-">co, </seg>
					<seg type="syl" met="-">pur'</seg>
					<seg type="syl" met="-">i </seg>
					<seg type="syl" met="+">bra</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>ed </seg>
					<seg type="syl" met="+">ar</seg>
					<seg type="syl" met="-">do</seg>
				</l>
				<l rhyme="e" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Per voi, che fie+ra quan+to bel+la se+te; =E -->
					<seg type="syl" met="-">Per </seg>
					<seg type="syl" met="-">voi, </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="+">fie</seg>
					<seg type="syl" met="-">ra </seg>
					<seg type="syl" met="+">quan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">bel</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">te;</seg>
				</l>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Ma la fiam+ma de l'al+ma_e_'l suo de-si+o =C+ -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">fiam</seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="+">l'al</seg>
					<seg type="syl" met="-">ma<c met="_"> </c>e<c met="_"> </c>'l </seg>
					<seg type="syl" met="-">suo </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">si</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="d" met="+--+---+-+-" n="endecasillabo">
					<!-- INPUT: Già+ non de-ri+va da so-a+ve sguar+do, =D -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">va </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">so</seg>
					<seg type="syl" met="+">a</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="+">sguar</seg>
					<seg type="syl" met="-">do,</seg>
				</l>
				<l rhyme="e" met="--+---+--+-" n="endecasillabo">
					<!-- INPUT: E non è+ quel che voi for+se cre-de+te. =E -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">è </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="+">for</seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">cre</seg>
					<seg type="syl" met="+">de</seg>
					<seg type="syl" met="-">te.</seg>
				</l>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Bra+mo sì,+ ma ven-det+ta: =a -->
					<seg type="syl" met="+">Bra</seg>
					<seg type="syl" met="-">mo </seg>
					<seg type="syl" met="+">sì, </seg>
					<seg type="syl" met="-">ma </seg>
					<seg type="syl" met="-">ven</seg>
					<seg type="syl" met="+">det</seg>
					<seg type="syl" met="-">ta:</seg>
				</l>
				<l rhyme="b" met="-----+---+-" n="endecasillabo">
					<!-- INPUT: E se pur dee gio-ir,+ non per a-mo+re =B -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">pur </seg>
					<seg type="syl" met="-">dee </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="+">ir, </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">a</seg>
					<seg type="syl" met="+">mo</seg>
					<seg type="syl" met="-">re</seg>
				</l>
				<l rhyme="b" met="---+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Ma per di-sde+gno_o-mai+ gio-i+sca_il co+re. =B -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">sde</seg>
					<seg type="syl" met="-">gno<c met="_"> </c>o</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">gio</seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">sca<c met="_"> </c>il </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re.</seg>
				</l>
			</lg>
		</body>
	</text>
</TEI>
