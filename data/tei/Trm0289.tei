	<text>
		<body>
			<lg>
				<l rhyme="a" met="+--+-+-+-+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Già+ tu vo-la+sti quat+tro vol+te_e se+i =A+ -->
					<seg type="syl" met="+">Già </seg>
					<seg type="syl" met="-">tu </seg>
					<seg type="syl" met="-">vo</seg>
					<seg type="syl" met="+">la</seg>
					<seg type="syl" met="-">sti </seg>
					<seg type="syl" met="+">quat</seg>
					<seg type="syl" met="-">tro </seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">i</seg>
				</l>
				<l rhyme="b" met="--+-++-" n="settenario" enjamb="yes">
					<!-- INPUT: In quel pet+to sì+ mol+le, =b+ -->
					<seg type="syl" met="-">In </seg>
					<seg type="syl" met="-">quel </seg>
					<seg type="syl" met="+">pet</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">sì </seg>
					<seg type="syl" met="+">mol</seg>
					<seg type="syl" met="-">le,</seg>
				</l>
				<l rhyme="a" met="+--+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Va+ga far-fal+la,_or mor+ta_al lu+me se+i. =A -->
					<seg type="syl" met="+">Va</seg>
					<seg type="syl" met="-">ga </seg>
					<seg type="syl" met="-">far</seg>
					<seg type="syl" met="+">fal</seg>
					<seg type="syl" met="-">la,<c met="_"> </c>or </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>al </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">se</seg>
					<seg type="syl" met="-">i.</seg>
				</l>
				<l rhyme="b" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: Non bra+mo_io lu+ce, né+ son tan+to fol+le; =B -->
					<seg type="syl" met="-">Non </seg>
					<seg type="syl" met="+">bra</seg>
					<seg type="syl" met="-">mo<c met="_"> </c>io </seg>
					<seg type="syl" met="+">lu</seg>
					<seg type="syl" met="-">ce, </seg>
					<seg type="syl" met="+">né </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">tan</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">fol</seg>
					<seg type="syl" met="-">le;</seg>
				</l>
				<l rhyme="a" met="--+--+-" n="settenario">
					<!-- INPUT: Ma la mor+te vor-re+i =a -->
					<seg type="syl" met="-">Ma </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">mor</seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">vor</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">i</seg>
				</l>
				<l rhyme="b" met="+--+-+---+-" n="endecasillabo">
					<!-- INPUT: Do+ve for-tu+na dar+la_a te non vol+le. =B -->
					<seg type="syl" met="+">Do</seg>
					<seg type="syl" met="-">ve </seg>
					<seg type="syl" met="-">for</seg>
					<seg type="syl" met="+">tu</seg>
					<seg type="syl" met="-">na </seg>
					<seg type="syl" met="+">dar</seg>
					<seg type="syl" met="-">la<c met="_"> </c>a </seg>
					<seg type="syl" met="-">te </seg>
					<seg type="syl" met="-">non </seg>
					<seg type="syl" met="+">vol</seg>
					<seg type="syl" met="-">le.</seg>
				</l>
				<l rhyme="c" met="-+-+-+-" n="settenario">
					<!-- INPUT: Oh dol+ce chiu+der gli_oc+chi, =c -->
					<seg type="syl" met="-">Oh </seg>
					<seg type="syl" met="+">dol</seg>
					<seg type="syl" met="-">ce </seg>
					<seg type="syl" met="+">chiu</seg>
					<seg type="syl" met="-">der </seg>
					<seg type="syl" met="+">gli<c met="_"> </c>oc</seg>
					<seg type="syl" met="-">chi,</seg>
				</l>
				<l rhyme="c" met="--+--+---+-" n="endecasillabo">
					<!-- INPUT: S'av-ver-rà+ che spi-ra+re_in lui mi toc+chi! =C -->
					<seg type="syl" met="-">S'av</seg>
					<seg type="syl" met="-">ver</seg>
					<seg type="syl" met="+">rà </seg>
					<seg type="syl" met="-">che </seg>
					<seg type="syl" met="-">spi</seg>
					<seg type="syl" met="+">ra</seg>
					<seg type="syl" met="-">re<c met="_"> </c>in </seg>
					<seg type="syl" met="-">lui </seg>
					<seg type="syl" met="-">mi </seg>
					<seg type="syl" met="+">toc</seg>
					<seg type="syl" met="-">chi!</seg>
				</l>
			</lg>
