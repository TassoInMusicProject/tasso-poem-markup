	<text>
		<body>
			<lg>
				<l rhyme="a" met="+-+--+-" n="settenario">
					<!-- INPUT: Don+na, lun+ge da vo+i =a -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na, </seg>
					<seg type="syl" met="+">lun</seg>
					<seg type="syl" met="-">ge </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">i</seg>
				</l>
				<l rhyme="b" met="+----+-" n="settenario">
					<!-- INPUT: Vi+vo del mio do-lo+re, =b -->
					<seg type="syl" met="+">Vi</seg>
					<seg type="syl" met="-">vo </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">do</seg>
					<seg type="syl" met="+">lo</seg>
					<seg type="syl" met="-">re,</seg>
				</l>
				<l rhyme="b" met="++-+---+-+-" n="endecasillabo">
					<!-- INPUT: Né+ man+ca_il ci+bo con la vi+ta_al co+re; =B -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="+">man</seg>
					<seg type="syl" met="-">ca<c met="_"> </c>il </seg>
					<seg type="syl" met="+">ci</seg>
					<seg type="syl" met="-">bo </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">vi</seg>
					<seg type="syl" met="-">ta<c met="_"> </c>al </seg>
					<seg type="syl" met="+">co</seg>
					<seg type="syl" met="-">re;</seg>
				</l>
				<l rhyme="c" met="-+---+-" n="settenario">
					<!-- INPUT: Per-ché+ da voi de-ri+va, =c -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">de</seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">va,</seg>
				</l>
				<l rhyme="c" met="-+-+-+-+-+-" n="endecasillabo">
					<!-- INPUT: E pa+re_un fiu+me sen+za fon+do_o ri+va. =C -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="+">pa</seg>
					<seg type="syl" met="-">re<c met="_"> </c>un </seg>
					<seg type="syl" met="+">fiu</seg>
					<seg type="syl" met="-">me </seg>
					<seg type="syl" met="+">sen</seg>
					<seg type="syl" met="-">za </seg>
					<seg type="syl" met="+">fon</seg>
					<seg type="syl" met="-">do<c met="_"> </c>o </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">va.</seg>
				</l>
				<l rhyme="d" met="-+-+-+-" n="settenario" enjamb="yes">
					<!-- INPUT: Voi sie+te_il fon+te,_e_'l ri+o =d+ -->
					<seg type="syl" met="-">Voi </seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">te<c met="_"> </c>il </seg>
					<seg type="syl" met="+">fon</seg>
					<seg type="syl" met="-">te,<c met="_"> </c>e<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">ri</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="d" met="--+--+++-+-" n="endecasillabo">
					<!-- INPUT: De la vo+stra bel-lez+za_è_'l+ pian+to mi+o. =D -->
					<seg type="syl" met="-">De </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">vo</seg>
					<seg type="syl" met="-">stra </seg>
					<seg type="syl" met="-">bel</seg>
					<seg type="syl" met="+">lez</seg>
					<seg type="syl" met="+">za<c met="_"> </c>è<c met="_"> </c>'l </seg>
					<seg type="syl" met="+">pian</seg>
					<seg type="syl" met="-">to </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">o.</seg>
				</l>
			</lg>
		</body>
	</text>
