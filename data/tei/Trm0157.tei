	<text>
		<body>
			<lg>
				<l rhyme="a" met="+----+-" n="settenario">
					<!-- INPUT: Don+na, nel mio ri-tor+no =a -->
					<seg type="syl" met="+">Don</seg>
					<seg type="syl" met="-">na, </seg>
					<seg type="syl" met="-">nel </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">ri</seg>
					<seg type="syl" met="+">tor</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="a" met="---+--+--+-" n="endecasillabo" enjamb="yes">
					<!-- INPUT: Il mio pen-sie+ro_a cui nul+la pon fre+no =A+ -->
					<seg type="syl" met="-">Il </seg>
					<seg type="syl" met="-">mio </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sie</seg>
					<seg type="syl" met="-">ro<c met="_"> </c>a </seg>
					<seg type="syl" met="-">cui </seg>
					<seg type="syl" met="+">nul</seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="-">pon </seg>
					<seg type="syl" met="+">fre</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="a" met="-+-+-++--+-" n="endecasillabo">
					<!-- INPUT: Pre-cor+re do+ve_il cie+lo_è+ più se-re+no, =A -->
					<seg type="syl" met="-">Pre</seg>
					<seg type="syl" met="+">cor</seg>
					<seg type="syl" met="-">re </seg>
					<seg type="syl" met="+">do</seg>
					<seg type="syl" met="-">ve<c met="_"> </c>il </seg>
					<seg type="syl" met="+">cie</seg>
					<seg type="syl" met="+">lo<c met="_"> </c>è </seg>
					<seg type="syl" met="-">più </seg>
					<seg type="syl" met="-">se</seg>
					<seg type="syl" met="+">re</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="a" met="---+-----+-" n="endecasillabo">
					<!-- INPUT: E se ne vie+ne_a far con voi sog-gior+no, =A -->
					<seg type="syl" met="-">E </seg>
					<seg type="syl" met="-">se </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">vie</seg>
					<seg type="syl" met="-">ne<c met="_"> </c>a </seg>
					<seg type="syl" met="-">far </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">sog</seg>
					<seg type="syl" met="+">gior</seg>
					<seg type="syl" met="-">no,</seg>
				</l>
				<l rhyme="b" met="+----+-" n="settenario">
					<!-- INPUT: Né+ da voi si di-par+te =b -->
					<seg type="syl" met="+">Né </seg>
					<seg type="syl" met="-">da </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">si </seg>
					<seg type="syl" met="-">di</seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">te</seg>
				</l>
				<l rhyme="a" met="-+-+-+-" n="settenario">
					<!-- INPUT: Gia-mai+ la not+te_e_il gior+no =a -->
					<seg type="syl" met="-">Gia</seg>
					<seg type="syl" met="+">mai </seg>
					<seg type="syl" met="-">la </seg>
					<seg type="syl" met="+">not</seg>
					<seg type="syl" met="-">te<c met="_"> </c>e<c met="_"> </c>il </seg>
					<seg type="syl" met="+">gior</seg>
					<seg type="syl" met="-">no</seg>
				</l>
				<l rhyme="b" met="-+-+--++-+-" n="endecasillabo">
					<!-- INPUT: Per-ché+ l'an-no+ia cias-cun'+al+tra par+te; =B -->
					<seg type="syl" met="-">Per</seg>
					<seg type="syl" met="+">ché </seg>
					<seg type="syl" met="-">l'an</seg>
					<seg type="syl" met="+">no</seg>
					<seg type="syl" met="-">ia </seg>
					<seg type="syl" met="-">cias</seg>
					<seg type="syl" met="+">cun'</seg>
					<seg type="syl" met="+">al</seg>
					<seg type="syl" met="-">tra </seg>
					<seg type="syl" met="+">par</seg>
					<seg type="syl" met="-">te;</seg>
				</l>
				<l rhyme="c" met="+----+--++-" n="endecasillabo">
					<!-- INPUT: On+de sol per vir-tù+ del pen-sier+ mi+o =C -->
					<seg type="syl" met="+">On</seg>
					<seg type="syl" met="-">de </seg>
					<seg type="syl" met="-">sol </seg>
					<seg type="syl" met="-">per </seg>
					<seg type="syl" met="-">vir</seg>
					<seg type="syl" met="+">tù </seg>
					<seg type="syl" met="-">del </seg>
					<seg type="syl" met="-">pen</seg>
					<seg type="syl" met="+">sier </seg>
					<seg type="syl" met="+">mi</seg>
					<seg type="syl" met="-">o</seg>
				</l>
				<l rhyme="c" met="+--+-----+-" n="endecasillabo">
					<!-- INPUT: Men+tre ne ven+go_a voi con voi son i+o. =C -->
					<seg type="syl" met="+">Men</seg>
					<seg type="syl" met="-">tre </seg>
					<seg type="syl" met="-">ne </seg>
					<seg type="syl" met="+">ven</seg>
					<seg type="syl" met="-">go<c met="_"> </c>a </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">con </seg>
					<seg type="syl" met="-">voi </seg>
					<seg type="syl" met="-">son </seg>
					<seg type="syl" met="+">i</seg>
					<seg type="syl" met="-">o.</seg>
				</l>
			</lg>
		</body>
	</text>
